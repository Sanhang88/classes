function newsubmitCorrection(type){
	if (loadallcerrectionjsflag) {
		submitCorrection(type);
	}
	else {
		jQuery.getScript("/js/allcerrection20120328.js", function(){
			submitCorrection(type);
			loadallcerrectionjsflag = true;
		});
	}
}

//å é¤å½ååç´ 
function removeNode(node){
	var pNode= node.parentNode;
	pNode.removeChild(node);
} 

var HdTools = {
	jsloadArray:[],
	loadJs: function(jsUrl, fun){// å è½½js
		var self = this;
		var callback = (typeof fun == 'function') ? fun : function(){};
		if(self.jsloadArray[jsUrl]){
			callback();
		}else{
			$.ajax({
				dataType:'script',
				url: jsUrl,
// scriptCharset: 'UTF-8',
// timeout: 5000,
				cache:true,
				success:function(){
					self.jsloadArray[jsUrl] = true;
					callback();
				}
			});
		}
	},
	loadingDialog: function(title, html, width, height, id) {
		var self = this;
		var loadingDialogId = ((typeof (id) == 'undefined') ? "HDloadingDialog"
				: id);
		if(html == null  || typeof(html) == 'undefined'){
			html = '<div style="padding:15px;"><img src="http://www.huimg.cn/entry/images/up.gif"/></div>';
		}
		$.dialog({
			id : loadingDialogId,
			title : title,
			content : html,
			overlay : 1,
			height : typeof (height) == 'undefined' ? "" : height,
			width : width,
			styleContent : {
				textAlign : 'center'
			},
			skin : "bluebox",
			callback : function(dialog) {
				dialog.css('zIndex', 10010);
				$("#hudong_overlay" + loadingDialogId).css('zIndex', 10000);
			},
			onClose : function() {
				$("#hudong_overlay" + loadingDialogId).hide();
			},
			resetTime : -1
		});
		// å»é¤dialogçclickäºä»¶ï¼é²æ­¢å¼¹çªçzindexéæ°è®¡ç®ï¼æ¡æ¶ä¸­å®ç°çï¼æ æ³æ´æ¹ï¼åªè½éè¿è¿ç§æ¹å¼è®¾ç½®ï¼
		$.dialog.get(loadingDialogId).unbind('click');
		// åçï¼å»é¤dialogä¸­çh2æ ç­¾ï¼æ é¢æ ï¼çç¸å³äºä»¶
		$.dialog.get(loadingDialogId).find("h2[class=title]").unbind(
				'mousedown');
	},
	closeLoadingDialog: function (id) {// å³é­å°å¼¹çª
		var self = this;
		var loadingDialogId = ((typeof (id) == 'undefined') ? "HDloadingDialog"
				: id);
		$.dialog.close(loadingDialogId);
	}
};
/**
 * å®åä¿¡æ¯æ¨¡åæä½
