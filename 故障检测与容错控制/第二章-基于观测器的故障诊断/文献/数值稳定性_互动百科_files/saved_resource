			     	  var $li =  $(".fodder-img li")[i];
			     	  fuTuLiLen = fuTuLiLen + $li.offsetWidth + liInterval;
			    }
			    
			    //å°å¤å çé´è·åæ
			    if(fuTuLiLen > 0){
			        fuTuLiLen  = fuTuLiLen  - liInterval;
			    }
			
			    var fuTuPageSize = Math.ceil(fuTuLiLen /782);     
