function postAsForm(url){
	var temp = document.createElement("form");
	temp.action = url; 
	temp.method = "POST"; 
	temp.style.visibility = "hidden"; 
	var opt = document.createElement("input"); 
	opt.name="editFlag"; 
	opt.id="editFlag"; 
	opt.value="docinfoeditcontent";
//	var opt2=document.createElement("input"); 
//	opt2.name="doc_title"; 
//	opt2.id="doc_title"; 
//	opt2.value=docTitle;
	var parentEle = document.getElementsByTagName("body")[0];
	parentEle.appendChild(temp);
	temp.appendChild(opt); 
//	temp.appendChild(opt2); 
	temp.submit(); 
}

function getCookie(name){
	var start = document.cookie.indexOf(name + "=");
	var len = start + name.length + 1;
	if ((!start) && (name != document.cookie.substring(0, name.length))) {
		return null;
	}
	if (start == -1) 
		return null;
	var end = document.cookie.indexOf(';', len);
	if (end == -1) 
		end = document.cookie.length;
	return unescape(document.cookie.substring(len, end));
}

function on_xnlogin(){
	XN.Connect.requireSession(function(){
		var href = document.getElementById('doctitle_utf8').value;
		window.location.href = 'http://www.baike.com/xiaoNeiConnectSuccess.do?action=show&type=1&url=' + href;
	});
}
function on_qzonelogin(){
	var doctitle = document.getElementById('doctitle_utf8').value;
	var url = "http://www.baike.com/wiki/" + doctitle;
	window.open('http://passport.baike.com/qzoneLogin.do?method=login&service=' + encodeURIComponent(url), 'QQç»é', 'fullscreen');
}

/**
 * å¯¹äºä¸å­å¨è¯æ¡ï¼æ çº¢çè¯æ¡æ§è¡å¼¹åºçªå£æä½
 */
function linkredwin(doc_title){
	var formredwin = document.createElement("form");
	formredwin.target = '_blank';
	formredwin.method = 'post';
	document.body.appendChild(formredwin);
	formredwin.action = "http://www.baike.com/wiki/" + encodeURI(doc_title);
	formredwin.submit();
	formredwin.parentNode.removeChild(formredwin);
}

/**
 * éå¾å®ä½
 */
function gotoFigure(){
	var urlstr = window.location.href;
	var iPos = urlstr.indexOf("#");
	if (iPos != -1) {
		var arr = new Array();
		arr = urlstr.split("#");
		if (arr.length == 2) {
			if (arr[1] == "_figurea") {
				window.location.hash = "#_figurea";
			}
			else {
				if (arr[1] == "forum") {
					$(document).scrollTop($(document).height());
				}
				else {
					if (arr[1] != '') {
						window.location.hash = arr[1];
					}
				}
			}
		}
	}
}

