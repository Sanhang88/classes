			register.style.display = "none";
			dialogTitleObj.html(ajaxLoginAndRegist.winTitleForLogin);
			ajaxLoginAndRegist.showSeccode('imgsec_login', 'login');
			$("#email_login").focus();
		}
		else 
			if (type == "REGIST") {
				ulli[1].className = "active";
				ulli[0].className = "";
				login.style.display = "none";
				register.style.display = "block";
				dialogTitleObj.html(ajaxLoginAndRegist.winTitleForRegist);
				ajaxLoginAndRegist.showSeccode('imgsec_regist', 'register');
				this.lastSeccode == "";
				$("#email_regist").focus();
				$("#email_regist").blur(function(){
					ajaxLoginAndRegist.checkEmail_reg();
				});
				$("#usernick").blur(function(){
					ajaxLoginAndRegist.checkNick_reg();
				});
				$("#pwd_regist").blur(function(){
					ajaxLoginAndRegist.checkPassword_reg(1);
				});
				$("#pwd2").blur(function(){
					ajaxLoginAndRegist.checkPassword_reg(2);
				});
				$("#seccode_regist").blur(function(){
					ajaxLoginAndRegist.checkSeccode_reg();
				});
			}
	},
	showSeccode: function(id, type){ // æ¾ç¤ºéªè¯ç 
		var objImage = document.getElementById(id);
		if (objImage != null) {
			str = "/Captcha.jpg?random=" + Math.random();
			if (type && type != '') {
				str += "&type=" + type;
			}
			objImage.src = str;
			if (type == "login") {
				$("#seccode_login").val("");
			}
			else 
				if (type == "register") {
					$("#seccode_regist").val("");
				}
		}
	},
	checkEmail_login: function(){ // æ£æ¥ç»å½ emaiåæ³æ§
		ajaxLoginAndRegist.clearMsg($("#msg_error_login"));
		var emailValue = $.trim($("#email_login").val());
		if (emailValue == "") {
			$("#msg_error_login").html(ajaxLoginAndRegist.errorInfo.error_email_empty);
			return false;
		}
		var accessEmail = '';
		var censorEmail = '';
		var accessXp = accessEmail != '' ? /()document.getElementById/i : null;
		var censoreXp = censorEmail != '' ? /()document.getElementById/i : null;
		illegalEmail = !(/^[\-\.\w]+@[\-\w]+(\.\w+)+$/.test(emailValue));
		invalidEmail = accessEmail != '' ? !accesseXp.test(emailValue) : censorEmail != '' && censoreXp.test(emailValue);
		if (illegalEmail || invalidEmail) {
			$("#msg_error_login").html(illegalEmail ? ajaxLoginAndRegist.errorInfo.error_email_illegal : (accessEmail != '' ? ajaxLoginAndRegist.errorInfo.error_email_invalid : ajaxLoginAndRegist.errorInfo.error_email_censor));
			return false;
		}
		return true;
	},
	checkPassword_login: function(){ // æ£æ¥ç»å½ å¯ç åæ³æ§
		ajaxLoginAndRegist.clearMsg($("#msg_error_login"));
		var password = $.trim($("#pwd_login").val());
		if (password == "") {
			$("#msg_error_login").html(ajaxLoginAndRegist.errorInfo.error_passwd_empty);
			return false;
		}
		return true;
	},
	checkSeccode_login: function(){ // æ£æ¥ç»å½ éªè¯ç åæ³æ§
		ajaxLoginAndRegist.clearMsg($("#msg_error_login"));
		var seccodeVerify = $.trim($("#seccode_login").val());
		if (seccodeVerify == "") {
			$("#msg_error_login").html(ajaxLoginAndRegist.errorInfo.error_seccode_empty);
			return false;
		}
		return true;
	},
	loginSubmit: function(){ // ç»å½æäº¤
		ajaxLoginAndRegist.clearMsg($("#msg_error_login"));
		if (ajaxLoginAndRegist.submitProcessFlag == true) {
			return;
		}
		var emailValue = $.trim($("#email_login").val());
		var pwdValue = $.trim($("#pwd_login").val());
		var seccodeValue = $.trim($("#seccode_login").val());
		if (ajaxLoginAndRegist.checkEmail_login() == false) {
			return false;
		}
		if (ajaxLoginAndRegist.checkPassword_login() == false) {
			return false;
		}
		if (ajaxLoginAndRegist.checkSeccode_login() == false) {
			return false;
		}
		ajaxLoginAndRegist.submitProcessFlag = true;
		$.ajax({
			type: "POST",
			url: "/ajaxDivLogin.do",
			dataType: "xml",
			data: {
				email: emailValue,
				passwd: hex_md5(pwdValue),
				seccode: seccodeValue,
				submittype: 1
			},
			success: function(xml){
				ajaxLoginAndRegist.submitProcessFlag = false;
				var eleid = $(xml).find("response > eleid").text();
				var errinfo = $(xml).find("response > errinfo").text();
				var submittype = $(xml).find("response > submittype").text();
				if (eleid == 'null' && errinfo == 'null') {
					// add by liaoxiandong 2010.11.30 ç»éæååèªå¨å°casServerä¸ç»è®°
					autoLoginToCasForPOP("login");
					$.dialog.close(ajaxLoginAndRegist.winId);
					setLogin();
					if (ajaxLoginAndRegist.editData.type == "SUMMARY") {
						DocSummaryEdit.requestEdit(ajaxLoginAndRegist.editData.docTitle);
					}
					else 
						if (ajaxLoginAndRegist.editData.type == "DOCALL") {
							editpower();
						}
						else 
							if (ajaxLoginAndRegist.editData.type == "DOCSECTION") {
								editpower(ajaxLoginAndRegist.editData.data);
							}
					
				}
				else 
					if (errinfo != 'null') {
						$("#msg_error_login").html(errinfo);
						return;
					}
			}
		});
	},
	changeagreeReg: function(){ // åææå¡æ¡æ¬¾æ£æ¥
		var chk = document.getElementById("chkagree");
		var btn = document.getElementById("btnReg");
		if (chk.checked) {
			btn.disabled = "";
		}
		else {
			btn.disabled = "disabled";
		}
	},
	lastEmail: "",
	lastNick: "",
	lastSeccode: "",
	checkForRegister: function(name, value){
		$.ajax({
			type: "GET",
			url: "/ajaxRegister.do?" + name + "=" + encodeURIComponent(value) + "&submittype=0",
			dataType: "xml",
			success: function(xml){
				var eleid = $(xml).find("response > eleid").text();
				var errinfo = $(xml).find("response > errinfo").text();
				var submittype = $(xml).find("response > submittype").text();
				if (eleid != 'null' && errinfo != 'null') {
					ajaxLoginAndRegist.errMsg($("#" + eleid + "_regist"), errinfo);
					return false;
				}
				else 
					if (submittype == '0' && eleid != 'null' && errinfo == 'null') {
						ajaxLoginAndRegist.infoMsg($("#" + eleid + "_regist"));
					}
				return true;
			}
		});
	},
	checkEmail_reg: function(){ // æ£æ¥æ³¨å emaiåæ³æ§
		var emailValue = $.trim($("#email_regist").val());
		if (emailValue == "") {
			ajaxLoginAndRegist.errMsg($("#div_email_regist"), ajaxLoginAndRegist.errorInfo.error_email_empty);
			return false;
		}
		var accessEmail = '';
		var censorEmail = '';
		var accessXp = accessEmail != '' ? /()document.getElementById/i : null;
		var censoreXp = censorEmail != '' ? /()document.getElementById/i : null;
		illegalEmail = !(/^[\-\.\w]+@[\-\w]+(\.\w+)+$/.test(emailValue));
		invalidEmail = accessEmail != '' ? !accesseXp.test(emailValue) : censorEmail != '' && censoreXp.test(emailValue);
		if (illegalEmail || invalidEmail) {
			ajaxLoginAndRegist.errMsg($("#div_email_regist"), illegalEmail ? ajaxLoginAndRegist.errorInfo.error_email_illegal : (accessEmail != '' ? ajaxLoginAndRegist.errorInfo.error_email_invalid : ajaxLoginAndRegist.errorInfo.error_email_censor));
			return false;
		}
		if (emailValue == this.lastEmail) {
			return;
		}
		else {
			this.lastEmail = emailValue;
		}
		if (this.checkForRegister("email", emailValue) == true) {
			return true;
		}
		return false;
	},
	checkNick_reg: function(){ // æ£æ¥æ³¨å æµç§°åæ³æ§
		var nick = $.trim($("#usernick").val());
		if (nick == "") {
			ajaxLoginAndRegist.errMsg($("#div_usernick_regist"), ajaxLoginAndRegist.errorInfo.error_usernick_empty);
			return false;
		}
		if (nick.match(/.*[^\w\u4E00-\u9FA5]+.*/)) {
			ajaxLoginAndRegist.errMsg($("#div_usernick_regist"), ajaxLoginAndRegist.errorInfo.error_usernick_illegal);
			return false;
		}
		var unlen = nick.replace(/[^\x00-\xff]/g, "**").length;
		if (unlen.length > 14) {
			ajaxLoginAndRegist.errMsg($("#div_usernick_regist"), ajaxLoginAndRegist.errorInfo.error_usernick_toolong);
			return false;
		}
		if (nick == this.lastNick) {
			return;
		}
		else {
			this.lastNick = nick;
		}
		if (this.checkForRegister("usernick", nick) == true) {
			return true;
		}
		return false;
	},
	checkPassword_reg: function(type){ // æ£æ¥æ³¨å å¯ç åæ³æ§
		var password = $.trim($("#pwd_regist").val());
		var repassword = $.trim($("#pwd2").val());
		if (password == "") {
			ajaxLoginAndRegist.errMsg($("#div_pwd_regist"), ajaxLoginAndRegist.errorInfo.error_passwd_empty);
			return false;
		}
		if (password.length < 6) {
			ajaxLoginAndRegist.errMsg($("#div_pwd_regist"), ajaxLoginAndRegist.errorInfo.error_passwd_tooshort);
			return false;
		}
		if (password.length > 16) {
			ajaxLoginAndRegist.errMsg($("#div_pwd_regist"), ajaxLoginAndRegist.errorInfo.error_passwd_toolong);
			return false;
		}
		if (/[\'\"\\]/.test(password)) {
			ajaxLoginAndRegist.errMsg($("#div_pwd_regist"), ajaxLoginAndRegist.errorInfo.error_passwd_illegal);
			return false;
		}
		ajaxLoginAndRegist.infoMsg($("#div_pwd_regist"));
		if (type == 2) {
			if (password != repassword) {
				ajaxLoginAndRegist.errMsg($("#div_pwd2_regist"), ajaxLoginAndRegist.errorInfo.error_passwd_notmatch);
				return false;
			}
			ajaxLoginAndRegist.infoMsg($("#div_pwd2_regist"));
			$("#pwd_regist").change(function(){
				ajaxLoginAndRegist.checkPassword_reg(2);
			});
		}
		return true;
	},
	checkSeccode_reg: function(){ // æ£æ¥æ³¨å éªè¯ç åæ³æ§
		var seccodeverify = $.trim($("#seccode_regist").val());
		if (seccodeverify == "") {
			ajaxLoginAndRegist.errMsg($("#div_seccode_regist"), ajaxLoginAndRegist.errorInfo.error_seccode_empty);
			return false;
		}
		if (seccodeverify == this.lastSeccode) {
			return;
		}
		else {
			this.lastSeccode = seccodeverify;
		}
		if (this.checkForRegister("seccode", seccodeverify) == true) {
			return true;
		}
		return false;
	},
	registSubmit: function(){ // æ³¨åæäº¤
		if (ajaxLoginAndRegist.submitProcessFlag == true) {
			return;
		}
		var emailValue = $.trim($("#email_regist").val());
		var nick = $.trim($("#usernick").val());
		var password = $("#pwd_regist").val();
		var seccodeVerify = $.trim($("#seccode_regist").val());
		
		if (ajaxLoginAndRegist.checkEmail_reg() == false) {
			return false;
		}
		if (ajaxLoginAndRegist.checkNick_reg() == false) {
			return false;
		}
		if (ajaxLoginAndRegist.checkPassword_reg(2) == false) {
			return false;
		}
		if (ajaxLoginAndRegist.checkSeccode_reg() == false) {
			return false;
		}
		ajaxLoginAndRegist.submitProcessFlag = true;
		$.ajax({
			type: "POST",
			url: "/ajaxDivRegister.do",
			dataType: "xml",
			data: {
				email: emailValue,
				usernick: nick,
				passwd: password,
				seccode: seccodeVerify,
				submittype: 1
			},
			success: function(xml){
				ajaxLoginAndRegist.submitProcessFlag = false;
				var eleid = $(xml).find("response > eleid").text();
				var errinfo = $(xml).find("response > errinfo").text();
				var submittype = $(xml).find("response > submittype").text();
				if (eleid == 'null' && errinfo == 'null') {
					// add by liaoxiandong 2010.11.30 æ³¨åæååèªå¨å°casServerä¸ç»è®°
					autoLoginToCasForPOP("register");
					$.dialog.close(ajaxLoginAndRegist.winId);
					setLogin();
					if (ajaxLoginAndRegist.editData.type == "SUMMARY") {
						DocSummaryEdit.requestEdit(ajaxLoginAndRegist.editData.docTitle);
					}
					else 
						if (ajaxLoginAndRegist.editData.type == "DOCALL") {
							editpower();
						}
						else 
							if (ajaxLoginAndRegist.editData.type == "DOCSECTION") {
								editpower(ajaxLoginAndRegist.editData.data);
							}
					
				}
				else 
					if (errinfo != "null") {
						ajaxLoginAndRegist.errMsg($("#" + eleid + "_regist"), errinfo);
						return;
					}
			}
		});
	},
	errMsg: function(obj, errinfo){ // æ¾ç¤ºéè¯¯ä¿¡æ¯
		$(obj).removeClass();
		$(obj).html(errinfo);
		$(obj).addClass("t_wrong");
	},
	clearMsg: function(obj){ // æ¸é¤æ¶æ¯æç¤º
		$(obj).removeClass();
		$(obj).parent().addClass('error');	// å å¥çº¢è²èæ¯
		$(obj).html("");
	},
	infoMsg: function(obj){ // æç¤ºæ­£ç¡®
		$(obj).removeClass();
		$(obj).html("");
		$(obj).addClass("t_right");
	}
};

/**
 * è¯æ¡è®¨è®ºåºå è½½
 */
var DocCommentLoad = {
	windowHeight: 0,
	handle: 0,
	num: 0,
	start: function(){
		var self = this;
		this.windowHeight = $(window).height();
		
		$(window).scroll(function(){
			if (self.num == 0) {
				clearTimeout(self.handle);
				self.handle = setTimeout(function(){
					self.load();
				}, 500);
			}
		});
		self.load();
	},
	load: function(){
		var self = this;
		var max = self.windowHeight + $(document).scrollTop(), top = $(".descriptionP").eq(0).offset().top - 400;
		if (top < max) {
			self.num++;
			self.loadComment();
		}
	},
	loadComment: function(){
		// åå§åè¯è®ºåº
		var id = $("#docid").val();
		var docTitle = $("#doctitle").val();
		var creatorIden = $("#creatoriden").val();
		var sourceUrl = window.location;
		var productDesc = "è¯æ¡";
		var iframeurl = "http://comment.baike.com/comment.do?action=show&p=1&pn=10&pc=01032301&ps=3&domain=" + encodeURIComponent(document.location) + "&id=" + id + "&json=" + encodeURIComponent('{"docTitle":"' + docTitle + '","creatorIden":"' + creatorIden + '"}') + "&sourceinfo=" + encodeURIComponent('{"sourceName":"' + docTitle + '","sourceUrl":"' + sourceUrl + '","productDesc":"'+productDesc+'"}');
		$("#commentIframeId").attr("src", iframeurl);
	}
};

function resizeImage(b, f, d) {
	if (b.width && b.height) {
		var c = parseInt(b.width);
		var a = parseInt(b.height);
		if ((f / c) <= (d / a)) {
			if (f < c) {
				b.width = f;
				b.height = (f / c) * a;
			} else {
				b.width = c;
				b.height = a;
			}
		} else {
			if (d < a) {
				b.height = d;
				b.width = (d / a) * c;
			} else {
				b.width = c;
				b.height = a;
			}
		}
	}
}

/**
 * ä¿¡æ¯æ¨¡åçè²
 */ 
function setTempletInfoColor(){
	if($('#docinfotemplettable').length>0){
		$('#docinfotemplettable > table > tbody > tr:odd').addClass('deepcolor');
	}
}

/**
 * èªå¨ç»éå°cas add by liaoxiandong 2010.12.06
 * 
 * @return
 */
function autoLoginToCasForPOP(type){
	var username ='';
	var password ='';
	var autologinTag ='';
	// è·ååæ°å¼
	if("login"==type){
		username=$.trim($("#email_login").val());
		password=$.trim($("#pwd_login").val());
		var obj = $("#issave");
		if(obj && obj.length >0){
			if(obj[0].checked ==true){
				autologinTag='true';
			}else{
				autologinTag='false';
			}
		}
	}else{
		username=$.trim($("#email_regist").val());;
		password=$("#pwd_regist").val();
		autologinTag='false';
	}
	var iframeSrc="http://passport.baike.com/login.do?service="+encodeURIComponent("http://www.baike.com/cas/autoLogin_simple.html")+"&loginTag=submit&un="+encodeURI(username)+"&autoLogin="+autologinTag+"&pw="+encodeURI(hex_md5(password));
	var iframeStr ="<iframe id=\"autologin2ServerIfr\" src=\""+iframeSrc+"\" width=\"0\" height=\"0\"></iframe>";
	if($("#autologin2ServerIfr").length >0){
		$("#autologin2ServerIfr").remove();
	}
	$("body").append(iframeStr);
}
/**
 * æ·»å å¾çæ¾å¤§é
 * 
 */
var DocImgLookUpTag = {
	findImg:function(){
		$('#content div').not($("table")).find('img').each(
// $('#content div img').each(
		function(i){
			//var imgUrl = $(this).attr('src');
			var imgUrl = $(this).attr('data-original');
			if(imgUrl.indexOf('att.hudong.com') > 0){
				try{
					DocImgLookUpTag.setTag(this);
				}catch(e){}
			}
		});
	},
	setTag:function(obj){
		var parentTag = $(obj).parent('a');
		var imgHref = parentTag.attr('href');
		imgHref+="?prd=zhengwenye_left_neirong_tupian";// ç»è®¡ä»£ç 
		parentTag.attr('href',imgHref);
		if(parentTag.length>0&&imgHref.indexOf('tupian.baike.com') > 0){
			var imgTitle = $(obj).attr('title');
			if(imgTitle=="null"||imgTitle==null||imgTitle.length<=0){
				imgTitle = parentTag.parent().find('strong').filter(":first").text();
				$(obj).attr('title',imgTitle);
			}
			var taghtml = '<a target="_blank" class="magnify" href="'+imgHref+'" name="enlargegif" id="enlargegif" title="æ¥çå¤§å¾"></a>';
			parentTag.after(taghtml);
		}	
	}
};

/**
 * ç¼è¾å°å¾åè½
 * 
 * @param flag
 * @param docTitle
 * @param maplongitude
 * @param maplatitude
 * @param mapdes
 * @param mapzoom
 */
function editMapRequest(flag, docTitle, maplongitude, maplatitude, mapdes,
		mapzoom) {
	var flag = false;
	if (flag && $("#MapabcJS").length > 0) {
		popEditMapWindow(flag, docTitle, maplongitude, maplatitude, mapdes,
				mapzoom);
	} else {
		var jsUrl= "http://www.huimg.cn/entry/js/mapmanager/mapmanager.js";
		if (loadmapmanagerjsflag) {
			popMapWindow(flag, docTitle, maplongitude, maplatitude, mapdes,
					mapzoom);
		} else {
			HdLoadJs(jsUrl, function() {
				loadmapmanagerjsflag = true;
				setTimeout(function(){
					popMapWindow(flag, docTitle, maplongitude, maplatitude, mapdes,
							mapzoom);
				}, 200);
			});
		}
	}
}

/**
 * ç¾ç§show
 */
function baikeShow(){
	var d_url="http://www.huimg.cn/lib/jquery.dialog-0.8.2.min.js";
var jsUrl = "http://www.huimg.cn/entry/js/docshow/docshow.js";
HdTools.loadJs(d_url, function(){
		HdTools.loadJs(jsUrl, function(){
		var doctitle = $('#doctitle').length > 0 ? $('#doctitle').val() : "";
		DocShow.tflag = null;
		DocShow.requestEdit(doctitle);
	});
	});

	
	return false;
}

/**
 * éç¨ä¸ä¼ å¾çåè°å½æ°
 * 
 * @param {}
 *            obj
 */
function processUploadImgCallback(obj) {
	if(obj!="undefined" && obj!=null && obj.type!="undefined" && obj.type!=""){
		if(obj.type=="11"){// æè¦éå¾åè°æ§è¡
			var msgobj = $("#summary_img_li p").eq(0);
			$(msgobj).html('<a href="javascript:void(0);" onclick="DocSummaryEdit.removePic();return false;">å é¤è¯¥å¾ç</a>');
			if (obj.flag == "ok") {
				var imgobj = $("#summary_img_li img").eq(0);
				$(imgobj).attr("src", obj.url);
				$(imgobj).attr("alt", "");
				$(imgobj).attr("title", "");
				if(jQuery.browser.msie && jQuery.browser.version <= 6){
					$(imgobj).load(function() {
						resizeImage(this, 120, 120);
					});
				}
			} else {
				HdDialog.loadingAlertDialogModel_1(obj.msg);
			}
			return;
		}else if(obj.type == "10"){//å¤§å¾ä¸ä¼ 
	if(obj.flag == "ok"){
	
	HdDialog.loadingAlertDialogModel_1("ä¸ä¼ æåï¼");
			$("#r_img").attr("src",obj.url);   
			
	}else{
	HdDialog.loadingAlertDialogModel_1("ä¸ä¼ å¤±è´¥ï¼è¯·éæ°ä¸ä¼ ï¼");
	return;
	}

			
			
		}
		else{
			if(obj.type == "12"){// ä¿¡æ¯æ¨¡åä¸ä¼ å¾çåè°æ§è¡
				if(obj.flag == "ok"){
					docInfoTemplet.uploadImgSucess(obj);
				} else {
					docInfoTemplet.uploadImgFailture(obj.msg);
				}
				return;
			}
		}
	}
}

/**
 * è¯æ¡é¡µé¢å·¥å·æ 
 */
var docToolbar = {
	_scroll:{},
	cataPos:[],
	cataMaxHeight: 215,
	_positionFixed : !(document.all)
				|| (((document.all) && (document.compatMode == "CSS1Compat") && (window.XMLHttpRequest))),
	init : function() {
		var self = this;
		var float_section_lis = $('#float_section ul li');
		if(null==float_section_lis||float_section_lis.length<1){
			return;
		}
		//å¯¹äºfixedå®ä½ï¼ä½æ¯ä¸æ³ä½¿ç¨åç½®cssçæåµ
		var adapt = docToolbar._positionFixed ? true : false;
		if(!adapt){
			$(document).find('body').css('background-image','url(about:blank)');
			$(document).find('body').css('background-attachment','fixed');
		}
		var fixedCss = {};
		if(docToolbar._positionFixed){
			fixedCss = { 
				position : 'fixed',
				bottom : '45px'
			}
		}else{
			fixedCss = {
				position : 'absolute',
				right : (($("#primary").length > 0) ? "-45px" : "-45px"),
				fixright : "0px"
			}
		}
		// çæè¯æ¡é¡µé¢çå·¥å·æ 
		var HdToolBar = $.HdToolBar({
			id : "tool",
			cssSupport : 1,
			//è¾å©èªå®ä¹fixedå®ä½
			selfFixed : adapt,
			$selfRightElement : $('.w-300'),
			$selfBottomElement : $('#index-footer'),
			isCalculateToobarWidth:1,
			css : fixedCss,
			appendExpr: (($("#primary").length > 0) ? "#primary" : "body"),
			standarExpr : (($("#primary").length > 0) ? "#primary" : "body"),
			html : self.getDocCatalogHtml(),
			css_offsety : 80,
			minLeft : 643,
			startline : 1700
		});
		self._scroll = HdToolBar._scroll;
		self.toolbarObj = HdToolBar._toolbarObj;
		var toolbarCatalog = self.toolbarObj.find("div.catalog");
		// è®¾ç½®åé¡¶é¨æé®äºä»¶
		self.toolbarObj.find(".topcontrol").bind("click", function() {
			$(document).scrollTop(0);
			$("div.full-list dd a").removeClass("current");
//			toolbarCatalog.scrollTop(0);
//			toolbarCatalog.hide();
			return false;
		});
		// è®¾ç½®ç®å½ç¹å»çæé®äºä»¶ï¼åæ¬ç®å½çæ¾ç¤ºåéèã
		self.toolbarObj.find(".toollist").bind("click", function() {
			if(toolbarCatalog.is(":hidden")){
			//	var cataObj = toolbarObj.find("div.full-list");
			//	if(cataObj.height() > self.cataMaxHeight){
			//		cataObj.height(self.cataMaxHeight+"px").css("overflow-y", "scroll");
			//	}
				toolbarCatalog.show();
			}
			else{
				toolbarCatalog.hide();
			}
			return false;
		});
		// è®°å½æ¯ä¸ªç®å½çä½ç½®
		$("#content h2,#content h3").each(function(i){
			self.cataPos[i] = $(this).offset().top;
		});
		// è®¾ç½®ç®å½çé«äº®ä½ç½®
		self.setCata(); 
		$(window).bind('scroll', function() {
			self.setCata();
		});
		$(window).bind('resize', function() {
			self.setCata();
		});
		//setTimeout("docToolbar.countHeight();", 6000);
	},
	//è®¡ç®æ®µè½çé«åº¦
	countHeight : function(){
		var topValue;
		$("#content h2,#content h3").each(function(i){
			topValue = $(this).offset().top;
			if(i == 0 && topValue == docToolbar.cataPos[i]){
				return false;
			}
			docToolbar.cataPos[i] = $(this).offset().top;
		});
	},
	/**
	 * å¤æ­å½åæ»å¨çä½ç½®åç®å½çä½ç½®ãç¶åå³å®å½åç®å½çå äº®æ¾ç¤º
	 */
	setCata: function(){
		var self = this;
		self.countHeight();
		var windowScroll = $(window).scrollTop();
		// ä¸ºäºåå°è¯¯å·®å¢å ä¸å®çåç§»éã
		//var adEle = $("#hd_ad_doc_29");
		windowScroll += 100;
		var cataindex = -1;
		var cataposarray =self.cataPos;
		for(var i=0; i<cataposarray.length; i++){
			if(windowScroll<cataposarray[i]){
				cataindex = i-1;
				break;
			}else{
				if(i == cataposarray.length-1){
					cataindex= i;
				}
			}
		}
		// å¦æcataindexæ¯è´å¼ãè¯´æä¸å¨ç®å½èå´åãåä¸è®¾ç½®ç¶æ
		if(cataindex == -1){
			return;
		}
		var className = "current";
		$("#tool div.full-list li").removeClass(className).eq(cataindex).addClass(className);
		// ç®å½æ¾ç¤ºæµ®å¨å®ä½
		var cataindexObj = $("#tool div.full-list li").eq(cataindex);
		if((cataindexObj.length > 0) && (typeof(cataindexObj.position()) != "undefined")){
			$("#tool div.full-list").scrollTop(cataindexObj.position().top - $("#tool div.full-list li").eq(0).position().top);
		}
		var scroll = self._scroll;
		if(scroll==null||scroll.length<1){
			return false;
		}
		scroll._resizeScorll();
	},
	/**
	 * è·åæ­¤æ¡çç®å½åå®¹
	 * 
	 * @returns 
	 */
	getDocCatalogHtml : function() {
		var html = "";
		var $float_section = $('#float_section').clone();
		if($float_section.find("li").length<1){
			// å¦ææç®å½çè¯åhtmlä¸­åæ¬åé¡¶é¨åç®å½
			// å¦ææ²¡æç®å½çè¯ï¼åhtmlåªæåé¡¶'é¨ä¸ä¸ªä¸è¥¿
			$float_section.find('div.catalog').empty();
		}
		//$float_section.find('div.catalog').hide();
		return html = $float_section.html();
	},
	catalogClickFun:function(ele){
		var len = navigationObj.isFloat ? 60:90;
		var key = $(ele).attr("catalogkey");
		var muluObj = $("a[name="+key+"]");
		if(muluObj && muluObj.length == 1){
			var top = muluObj.offset().top -len;
			$(document).scrollTop(top);
			$("div.full-list li").removeClass("current");
			$(ele).parent('li').addClass("current");
		}
	}
};


function checkDateNowForWinAd(s,e){
	var now = Date.parse(new Date());
	if(now<Date.parse(s)||now>Date.parse(e)){
		return false;
	}
	return true;
}

/**
 * ç¨æ·å¤´è¡è®¾ç½®
 */
var userTitlesConfig = {
	uidens: [],
	setOneUser: function(iden){
		var self = this;
		self.uidens = [];
		self.uidens.push(iden);
		self.action();
	},
	init: function(){
		var self = this;
		self.getAllUIden();
		self.action();
	},
	action: function(uidens){
		var self = this;
		var url = "http://i.baike.com/userCardInfo.do?action=getUserCards&userIdens=" + self.uidens.join(";") + "&callback=?";
		$.getJSON(url, function(json){
			// å¦æè¿åæ°æ®å¼å¸¸
			if(json == null || json.flag == null) {
				return;
			}
			var flag = json.flag;
			// åå§åmomoForVEditor
			MomoForVEditor.init();
//			if (flag == 1 && json.userTitles!=null){
				self.setTitles(json.userTitles, json.showSwitchs);
			
			//ä¸å®¶åºåä¸­çå¿åç¨æ·åç¼æ ç­¾è®¾ç½®
			var attr = $(".zhuanjia ul li a");
			$.each(attr, function(i, aObj){
			  var aTxt = aObj.innerText;
			  if(aTxt == "å¿åç¨æ·"){
			     	var html = self.getHtml({displayType:2,showLink:"",titleName:"äºå¨ç¾ç§ç½å",showTitle:"äºå¨ç¾ç§ç½å",showIco:'yyico',showMemo:''});
		    	$("'"+html+"'").insertAfter(aObj.parentNode);
			 }
		 });
		 
			//åè£æ¦ä¸­çå¿åç¨æ·åç¼æ ç­¾è®¾ç½®
			var attr = $(".gongxianbang ul li a");
			$.each(attr, function(i, aObj){
			  var aTxt = aObj.innerText;
			  if(aTxt == "å¿åç¨æ·"){
			     	var html = self.getHtml({displayType:2,showLink:"",titleName:"äºå¨ç¾ç§ç½å",showTitle:"äºå¨ç¾ç§ç½å",showIco:'yyico',showMemo:''});
		    	$("'"+html+"'").insertAfter(aObj.parentNode);
			 }
		 });
				
//			}
		});
	},
	getAllUIden: function(){
		var self = this;
		var temArr = [];
		$("a[uiden]").each(function(i){
			 $(this).addClass("vEditor");
			var uiden = $(this).attr("uiden");
			
			// å»éå¤ç
			if((temArr[uiden]!=null) && (temArr[uiden])){
				
			}else{
				self.uidens.push(uiden);
				temArr[uiden]=true;
			}
		 });
	},
	setTitles: function(userTitles, showSwitchs){
		var self = this;
//		if(userTitles==null){
//			return;
//		}
		$.each(self.uidens, function(i, userIden){
			if(userTitles != null && userTitles[userIden] !=null){
				//edit by yangzaizhong ææç¨æ·é½éè¦æ¾ç¤º
				//self.setTitle(userIden, userTitles[userIden], (showSwitchs==null)? "0":showSwitchs[userIden]);
				self.setTitle(userIden, userTitles[userIden], "1");
			}else{
				self.setTitle(userIden, null, null);
			}
		});
	},
	setTitle: function(userIden, userTitles, showSwitch){
		var self = this;
		var jqueryObj = $(".rightdiv a[uiden="+userIden+"]");
		$(".rightdiv a[uiden="+userIden+"]").parent().find("a").not("a[uiden="+userIden+"]").remove();
		var html="";
		if(userTitles== null){
			//add by yangzaizhong ææç¨æ·é½éè¦æ¾ç¤º
			//jqueryObj.addClass("bold vEditor");
			html += self.getHtml({displayType:2,showLink:"",titleName:"å¤§å­¦ç",showTitle:"å¤§å­¦ç",showIco:'yyico',showMemo:''});
			$("'"+html+"'").insertAfter(jqueryObj.parent());
			return;
		}
		var flag = true;
		$.each(userTitles, function(i, userTitle){
			if(userTitle.showIco=='zyrzico'){
				var htmltemp = self.getHtml(userTitle);
				if(htmltemp!=""){
				  $("'"+htmltemp+"'").insertAfter(jqueryObj);
			  }
			}
			if(userTitle.showIco=='yyico'){
				html += self.getHtml(userTitle);
				flag=false;
			}
		});

		if(flag){
			html += self.getHtml({displayType:2,showLink:"",titleName:"å¤§å­¦ç",showTitle:"å¤§å­¦ç",showIco:'yyico',showMemo:''});
		}
		if((showSwitch!=null) && (showSwitch == "1")){
			//jqueryObj.addClass("bold vEditor");
		}
		if(html!=""){
			$("'"+html+"'").insertAfter(jqueryObj.parent());
		}
		// ä¸ºä¸å®¶è¯å®¡ä½¿ç¨
		var dianpingObject = $(".dianping a[uiden="+userIden+"]");
		$(".dianping a[uiden="+userIden+"]").parent().find("a").not("a[uiden="+userIden+"]").remove();
		if(dianpingObject.length > 0){
			$("'"+self.getHtml(userTitles[0])+"'").insertAfter(dianpingObject);
			if((showSwitch!=null) && (showSwitch == "1")){
				dianpingObject.addClass("bold vEditor");
			}
		}
	},
	getHtml: function(userTitle){
		var html='';
		if(userTitle == null){
			return html;
		}

		var displayType = userTitle.displayType;
		var titleName = userTitle.titleName;
		var showTitle = userTitle.showTitle;
		var showIco = userTitle.showIco;
		var showLink = userTitle.showLink;
		var showMemo = userTitle.showMemo;
		if(displayType == 1){
			if(showLink == "" ){
				html = '<a title="'+titleName+'" class="'+showIco+'"></a>';
			}else{
				html = '<a href="'+showLink+'" target="_blank" title="'+titleName+'" class="'+showIco+'"></a>';			
			}
		}
		if(displayType == 2){
			if(showLink == "" ){
				html = '<a title="'+titleName+'" class="'+showIco+'">'+showTitle+'</a>';
			}else{
				html = '<a href="'+showLink+'" target="_blank" title="'+titleName+'" class="'+showIco+'">'+showTitle+'</a>';			
			}
		}
		return html;
	}
};


function updateFirstExpert(uNick, uIden){
	if($(".zhuanjia").length > 0){
		// å°åæ¥çèä¸å®¶ä¸ç§»
		if($(".zhuanjia ul li").length>0){
			var firstExpertA= $("#firstExpertA");
			var oldFirstExpertNick = firstExpertA.text();
			var oldFirstExpertIden = firstExpertA.attr("uiden");
			// å¦æèä¸å®¶ä¸ºå½åè¦æ·»å çä¸å®¶
			if(oldFirstExpertIden == uIden) {
				return;				
			}
			var templet = $('<li><p><a uiden="" href="" target="_blank" class="yonghu"></a></p></li>');
			var html = templet.clone();
			html.find("p a").attr("uiden", oldFirstExpertIden).attr("href", "http://i.baike.com/profile.do?useriden="+oldFirstExpertIden).text(oldFirstExpertNick);
			html.insertBefore($(".zhuanjia ul li").eq(0));
			$("#firstExpertA").parent().find("a").not(":first").insertAfter($(".zhuanjia ul li").eq(0).find("p a"));
		}
		// çææ°çä¸å®¶
		$("#firstExpertA").attr("uiden", uIden).attr("href", "http://i.baike.com/profile.do?useriden="+uIden).text(uNick);
		$("#firstExpertA").parent().find("a").not(":first").remove();
		$("#firstExpertP").html();
		$("#firstExpertImg").attr("src", "http://www.huimg.cn/entry/images/50loading.gif");
		$("#firstExpertImg").parent().attr("href", "http://i.baike.com/profile.do?useriden="+uIden);
		setFirstExpertInfo();
		userTitlesConfig.setOneUser(uIden);
	}
}

/**
 * è®¾ç½®è®¾ç½®ç¬¬ä¸ä¸ªæå¨ä¸å®¶çä¿¡æ¯
 */
function setFirstExpertInfo(){
	// è®¾ç½®ç¬¬ä¸ä¸ªæå¨ä¸å®¶çä¿¡æ¯
	var userIden = $("#firstExpertA").attr("uiden");
	
	if (MomoForVEditor.cache_list[userIden]) {
		setInfo(MomoForVEditor.cache_list[userIden]);
		return;
	}
	
	function setInfo(data){
		// å°è·åçæ°æ®è¿è¡ç¼å­ï¼é¿åéå¤é¾æ¥
		MomoForVEditor.cache_list[userIden] = data;
		if(data.userImg!=null){
			$("#firstExpertImg").attr("src", data.userImg);
		}
		if(data.first!=null){
			var firstFirst = data.first[0];
			  $("#firstExpertP").text(firstFirst.authenticationInfo);
		}else{
				$("#firstExpertP").html('&nbsp;');
		}
	}
	
	var url = "http://i.baike.com/userCardInfo.do?userIden=" + userIden + "&callback=?";
	$.getJSON(url, function(json){
		// å¦æè¿åæ°æ®å¼å¸¸
		if(json == null || json.flag == null) {
			return;
		}
		
		var flag = json.flag;
		if (flag == 1){
			setInfo(json);
		}
	});
}

// ////////////////////////ä»¥ä¸ä¸å®¶ç¹è¯ç¸å³///////////////////////////
/**
 * è¯åæä½
 * 
 * @param {Object}
 *            elem
 * @param {Object}
 *            popWindow æææå±å¼¹æ¡å¯¹è±¡ ExpertCommentaryPannel111116.js
 * @memberOf {TypeName}
 * @return {TypeName}
 */
function StarMark(elem, popWindow) {
	var t = this;
	t.body = $(elem);
	t.originScore = t.body.attr('class').match(/^.*\s+s(.*)$/)[1];
	t.score = t.originScore;
	t.star = $("a",t.body);
	t.scoreCss = ['mark', 'mark s1', 'mark s2', 'mark s3',
			'mark s4', 'mark s5','mark s6','mark s7','mark s8','mark s9','mark s10'];
	t.markStar = function(score) {
			lightStar(score);
		t.score = score;
	};
	t.resetStar = function() {
		t.markStar(t.originScore);
	};
	// æ­¤å½æ°æ¯å½æ´æ°è¯æ¡åå¼æååçæä½
	t.truelyMarkStar = function(score) {
		t.originScore = score;
		t.markStar(score);
	};
	function lightStar(score) {
		t.body.attr('class', t.scoreCss[score]);
	}
	function addMarkEvent(el, score) {
		score++;
		el.click(function() {
					// å¦æå½åæ²¡æå¯¹ä¸å®¶ç¹è¯è¿è¡æä½
					if(isWaitingExpertAction == false) {
						// ç½®ä¸ºæ­£å¨æä½
						isWaitingExpertAction = true;
						
						// å½é¡µé¢ä¸ææç¹å»äºä»¶
						if(popWindow == null) {
							// åéç»è®¡äºä»¶
							expertCommentaryEvent.sendEventLog(expertCommentaryEvent.SCORE_CLICK);
							validateExpertsPermission(
								$("#doctitle").val(), 
								'SCORING_DOC', 
								1, 
								function() {
									t.markStar(score);
									HdTools.loadJs("http://www.huimg.cn/entry/js/expertCommentaryPannel/ExpertCommentaryPannel.js", function(){
										new ExpertCommentaryPannel($("#doctitle").val(), t.score, null, null).show();
										
										// ç½®ä¸ºæä½å®æ
										isWaitingExpertAction = false;
									});
								},
								function() {
									// åéç»è®¡äºä»¶
									expertCommentaryEvent.sendEventLog(expertCommentaryEvent.SCORE_NO_PERMISSION);

									// ç½®ä¸ºæä½å®æ
									isWaitingExpertAction = false;
								});
						}else {
							t.markStar(score);
							// æ­¤å¤ä¸ºæµ®çªä¸çææç¹å»äºä»¶
							popWindow.needUpdateScore = true;
							popWindow.score = t.score;

							// ç½®ä¸ºæä½å®æ
							isWaitingExpertAction = false;
						}
					}
				});
		el.mouseover(function() {
					lightStar(score);
				});
		el.mouseout(function() {
					lightStar(t.score);
				});
	}
	for (var i = 0; i < t.star.length; i++) {
		addMarkEvent($(t.star[i]), i);
	}
	
	
}

// å®ä¹è¯åå¯¹è±¡
var hddoc_starmark_page = null;
var hddoc_starmark_dialog = null;
// å®ä¹å¨å±åéï¼é¡µé¢ä¸ä¸å®¶è¯å®¡æä½æ¯å¦å®æ
var isWaitingExpertAction = false;

/**
 * åå§åææçåä¸å®¶ç¹è¯ç¸å³ä»£ç 
 */
function initExpertCommentary() {
	// åå§åæåä»£ç 
	if($('div.content-h1 div.scoreAction').length>0){
		hddoc_starmark_page = new StarMark($('div.content-h1 div.scoreAction'), null);
	}
	
	// åå§åæå¨è¯å®¡äºä»¶
	$('.quanweipingshen').click(function() {
		// å¦æå½åæ²¡æå¯¹ä¸å®¶ç¹è¯è¿è¡æä½
		if(isWaitingExpertAction == false) {
			// ç½®ä¸ºæ­£å¨æä½
			isWaitingExpertAction = true;
			
			// ç­å¾æµ®çªå¼å¯
			HdTools.loadingDialog("loading",null,300,100);
	
			// åéç»è®¡äºä»¶
			expertCommentaryEvent.sendEventLog(expertCommentaryEvent.ADDOWNER_CLICK);
			$.ajax(
			{
					type: 'POST',
					async: true,
					url: 'http://www.baike.com/wikdoc/sp/docExpert.do?method=addOwner',
					timeout:5000,
					data: {
						docTitle : $("#doctitle").val(),
						docId : $("#docid").val()
					},
					success: function(data) {
						if(data != null && data.substr(0, 10) == "DO_SUCCESS") {
							// åéç»è®¡äºä»¶
							expertCommentaryEvent.sendEventLog(expertCommentaryEvent.ADDOWNER_SUCCESS);
							// å±ç¤ºç¹è¯æµ®çª
							HdTools.loadJs("http://www.huimg.cn/entry/js/expertCommentaryPannel/ExpertCommentaryPannel.js", function(){
								// å³é­ç­å¾æµ®çª
								HdTools.closeLoadingDialog();
								
								new ExpertCommentaryPannel($("#doctitle").val(), null, true, function() {
									// æ´æ°å³ä¾§è®¤é¢åè¡¨æ°æ®
									updateFirstExpert(data.split("|")[2], data.split("|")[1]);
								}).show();
							});
						}else {
							// åéç»è®¡äºä»¶
							expertCommentaryEvent.sendEventLog(expertCommentaryEvent.ADDOWNER_FAILURE);
							// å³é­ç­å¾æµ®çª
							HdTools.closeLoadingDialog();
							if(data == null || data == "") {
								fadeOutTips("éå¸¸æ±æ­ï¼æä½å¤±è´¥", 1);
							}else if(data == "UN_LOGIN") {
								dealWithLogin();
							}else if(data == "DOC_NOT_EXIST") {
								fadeOutTips("éå¸¸æ±æ­ï¼è¯¥è¯æ¡å·²å é¤", 1);
							}else if(data == "INVALID_DOC_STATE") {
								fadeOutTips("éå¸¸æ±æ­ï¼è¯¥è¯æ¡ç®åæ æ³è¿è¡æå¨è¯å®¡", 1);
							}else if(data == "NO_PERMISSION") {
								// åéç»è®¡äºä»¶
								expertCommentaryEvent.sendEventLog(expertCommentaryEvent.ADDOWNER_NO_PERMISSION);
								
								var content = '<div class="qwdp_open qwdp_open_nops w-400"><p>æ±æ­ï¼å½ååªæ <span class="bold"><a href="http://www.baike.com/wiki/%E4%BA%92%E5%8A%A8%E7%99%BE%E7%A7%91%EF%BC%9A%E4%B8%93%E4%B8%9A%E8%AE%A4%E8%AF%81%E6%99%BA%E6%84%BF%E8%80%85?prd=quanweipingshen_dialog" target="_blank">ä¸ä¸è®¤è¯æºæ¿è</a></span> å¯ä»¥å¯¹è¯æ¡è¿è¡æå¨è¯å®¡ï¼</p><dl><dt class="bold">æå¨è¯å®¡ä¹åï¼æ¨å¯ä»¥ï¼</dt><dd>ç»è¯æ¡æåï¼ï¼æ¨çæåä¼ç«å»åºç°å¨è¯æ¡é¡µä¸ï¼</dd><dd>å¯¹è¯æ¡è¿è¡æå¨ç¹è¯ï¼ï¼æ¨çç¹è¯ä¼ç«å»åºç°å¨è¯æ¡é¡µä¸ï¼</dd><dd>å¯¹è¯æ¡åçæ¬åå®¹è¿è¡è¯å®¡ï¼ï¼æ¨å¯ä»¥å¯¹å½åè¯æ¡çåä¸ªçæ¬è¿è¡ç¹è¯ç­æä½ï¼çè³å é¤ï¼ï¼</dd></dl></div>';
                HdDialog.loadingDialogModel_1("æå¨è¯å®¡", content, 400, 200);

							}else if(data == "FAILURE") {
								fadeOutTips("éå¸¸æ±æ­ï¼æä½å¤±è´¥", 1);
							}else if(data == "DO_FAILURE") {
								fadeOutTips("éå¸¸æ±æ­ï¼æä½å¤±è´¥", 1);
							}
						}
						// ç½®ä¸ºæä½å®æ
						isWaitingExpertAction = false;
					},
					error : function() {
						// å³é­ç­å¾æµ®çª
						HdTools.closeLoadingDialog();
						fadeOutTips("éå¸¸æ±æ­ï¼ç±äºç½ç»åå æä½å¤±è´¥", 1);

						// ç½®ä¸ºæä½å®æ
						isWaitingExpertAction = false;
					},
					dataType: 'text'
				}
			);
		}
	});
	
	// åå§å"ææ¥ç¹è¯"ä»£ç 
	$('.expertCommentA').click(function() {
		// å¦æå½åæ²¡æå¯¹ä¸å®¶ç¹è¯è¿è¡æä½
		if(isWaitingExpertAction == false) {
			// ç½®ä¸ºæ­£å¨æä½
			isWaitingExpertAction = true;
			
			// åéç»è®¡äºä»¶
			expertCommentaryEvent.sendEventLog(expertCommentaryEvent.COMMENT_CLICK);
			validateExpertsPermission(
							$("#doctitle").val(), 
							'COMMENT_DOC', 
							2,
							function() {
							HdTools.loadJs("http://www.huimg.cn/entry/js/expertCommentaryPannel/ExpertCommentaryPannel.js", 	function(){
									new ExpertCommentaryPannel($("#doctitle").val(), null, null, null).show();
								 });
								
								// ç½®ä¸ºæä½å®æ
								isWaitingExpertAction = false;
							},
							function() {
								// åéç»è®¡äºä»¶
								expertCommentaryEvent.sendEventLog(expertCommentaryEvent.COMMENT_NO_PERMISSION);

								// ç½®ä¸ºæä½å®æ
								isWaitingExpertAction = false;
							}
						);
		}
	});
		
}

/**
 * éªè¯æé
 */
function validateExpertsPermission(docTitle, productCodeEnum, place, onSuccess, onError) {
	// ä¼åæ¬å°æªç»å½å¤æ­
	if (getHdCookie(hdCookieName) == null) {
		dealWithLogin();
		return ;	// èµ°ä¸å°è¿æ­¥ï¼loginéè·³è½¬äº
	}
	
	$.ajax(
		{
			type: 'POST',
			async: true,
			url: 'http://www.baike.com/wikdoc/sp/docExpert.do?method=validate',
			timeout: 5000,
			data: {
				docTitle :docTitle, 
				productCodeEnum : productCodeEnum
			},
			success: function(data) {
				if(data == "SUCCESS") {
					if(onSuccess != null) {
						onSuccess.call();
					}
				}else {
					if(data == null || data == "") {
						fadeOutTips("éå¸¸æ±æ­ï¼æä½å¤±è´¥", place);
					}else if(data == "UN_LOGIN") {
						dealWithLogin();
					}else if(data == "DOC_NOT_EXIST") {
						fadeOutTips("éå¸¸æ±æ­ï¼è¯¥è¯æ¡å·²å é¤", place);
					}else if(data == "INVALID_DOC_STATE") {
						fadeOutTips("éå¸¸æ±æ­ï¼è¯¥è¯æ¡ç®åæ æ³è¿è¡æå¨è¯å®¡", place);
					}else if(data == "NO_PERMISSION") {
						fadeOutTips("åªæè®¤è¯ç¨æ·æå¯ä»¥è¿è¡è¯æ¡ç¹è¯", place);
					}else if(data == "FAILURE") {
						fadeOutTips("éå¸¸æ±æ­ï¼æä½å¤±è´¥", place);
					}
					// åè°éè¯¯å½æ°
					if(onError != null) {
						onError.call();
					}
				}
			},
			error : function() {
				fadeOutTips("éå¸¸æ±æ­ï¼ç±äºç½ç»åå æä½å¤±è´¥", place);
				// åè°éè¯¯å½æ°
				if(onError != null) {
					onError.call();
				}
			},
			dataType: 'text'
		}	
	);
}
/**
 * å¤çç»éæä½
 */
function dealWithLogin() {
	var url =  "http://passport.baike.com/login.do?site_ource=zz_page&service=" + encodeURIComponent("http://www.baike.com/wiki/" + encodeURI($("#doctitle").val()));
	window.location.href = url;
	
}
/**
 * è¯æ¡é¡µé¢ä¸å®¶ç¹è¯çä¸¤ä¸ªä½ç½®ä¸çæ·¡å¥æ·¡åºæç¤ºæ¡
 * 
 * @param {Object}
 *            content
 * @param {Object}
 *            place
 * @memberOf {TypeName}
 */
function fadeOutTips(content, place) {
	if(place == 1) {
		$('<div class="qwdp_open_nodp">'+ content +'</div>').insertBefore($('.quanweipingshen')).animate({opacity: 1.0}, 1000).fadeOut("slow",function(){ 
			$(this).remove(); 
		}); 
	}else {
		$('<div class="qwdp_open_nodp">'+ content +'</div>').insertAfter($('.expertCommentA')).animate({opacity: 1.0}, 1000).fadeOut("slow",function(){ 
			$(this).remove(); 
		}); 
	}
}

/**
 * æ´æ°é¡µé¢ææ°ç¹è¯è®°å½
 * 
 * @param {Object}
 *            userIden
 * @param {Object}
 *            userNick
 * @param {Object}
 *            commentContent
 */
function updatePageCommentary(userIden, userNick, commentContent) {
	var commentPannel = $('.dianping');
	if(commentPannel.size() > 0) {
		$('.yonghu', commentPannel).attr("uiden", userIden);
		$('.yonghu', commentPannel).attr("href", "http://i.baike.com/profile.do?useriden=" + userIden);
		$('.yonghu', commentPannel).text(userNick);
		$('.yonghu', commentPannel).attr('class', 'yonghu');
		$('.yonghu', commentPannel).unbind();
		
		// æ´æ°è¯è®ºåå®¹
		$($('span', commentPannel).get(1)).html('<em class="yinhao_l"></em>'+ commentContent +'<em class="yinhao_r"></em>');
		
		userTitlesConfig.setOneUser(userIden);
	}else {
		setTimeout(function() {
			window.location.reload();
		}, 800);
	}
}

var expertCommentaryEvent = {
	PREFIX:"citiaozhengwen_expert",
	SCORE_CLICK:"score_click",
	SCORE_SUBMIT:"score_submit",
	SCORE_SUCCESS:"score_success",
	SCORE_FAILURE:"score_failure",
	SCORE_NO_PERMISSION:"score_no_permission",
	COMMENT_CLICK:"comment_click",
	COMMENT_SUBMIT:"comment_submit",
	COMMENT_SUCCESS:"comment_success",
	COMMENT_FAILURE:"comment_failure",
	COMMENT_NO_PERMISSION:"comment_no_permission",
	ADDOWNER_CLICK:"addowner_click",
	ADDOWNER_SUCCESS:"addowner_success",
	ADDOWNER_FAILURE:"addowner_failure",
	ADDOWNER_NO_PERMISSION:"addowner_no_permission",
	sendEventLog:function(flag){
		recordLog(expertCommentaryEvent.PREFIX+"_"+flag);
	}
};
// /////////////////////å·²ä¸ä¸å®¶ç¹è¯ç¸å³//////////////////////////////////////////
// å¯¹ç¹æ®å­ç¬¦è¿è¡æ£æ¥ ä¾å¦+ã&
function checkURIValue(doctitle) {
		if (doctitle == null || doctitle.length <=0) {
			return "";
		}else if(true == checkDocTitle(doctitle)){
			doctitle = doctitle.replace(/(^\s*)|(\s*$)/g, "");
			doctitle = doctitle.replace(/\/|\%|\\/g, "");
			doctitle = doctitle.replace(/\&/g, "%26");
			doctitle = doctitle.replace(/\+/g, "%2B");
		}else{
			doctitle = encodeURI(doctitle);
		}		
		return doctitle;
	}
// æ£æ¥doctitleæ¯å¦å«æç¹æ®å­ç¬¦
function checkDocTitle(doctitle){
	var reg =/[\+|\#|\&]+/;
	if(reg.test(doctitle)){
		return true;
	}
	return false;
}
// ç¹æ®å­ç¬¦çdoctitleä¸æ¬¡ç¼ç 
function checkURIDotitle(doctitle){
	if (doctitle == null || doctitle.length <=0) {
			return "";
		}else if(true == checkDocTitle(doctitle)){
			doctitle = doctitle.replace(/(^\s*)|(\s*$)/g, "");
			doctitle = doctitle.replace(/\/|\%|\\/g, "");
			doctitle = doctitle.replace(/\&/g, "%26");
			doctitle = doctitle.replace(/\+/g, "%2B");
			return encodeURIComponent(doctitle);
		}else{
			return encodeURI(doctitle);
		}		
}

var navigationObj = {
  objHeight:232,
  topElement:null,// æ­£å¸¸ç¶æçå¯¼èª
  floatElement :null,// æ¼æµ®ç¶æçå¯¼èª
  isFloat : false,
  timeout : false,
  primary_top:0,
  headHeight : 0,
  init : function(){
	navigationObj.topElement = $("div.mainnav-l:first");  //é¡¶é¨å¯¼èª + äºç»´ç 
	navigationObj.objHeight =  navigationObj.topElement.offset().top;  //è·ååç§»é¡¶é¨é«åº¦
	navigationObj.floatElement = $("div.mainnav-wrap");			 //æµ®å¨å¯¼èªæ 
	navigationObj.headHeight = $("div.header-all:first").height();  //å¤´é¨é«åº¦
	var primary_div = $("#primary");   //æ´ä¸ªé¡µé¢
	var kmodDiv = $("div[name=div_fun_kmod]");  //ç¥è¯æ¨¡åä¸æè¡¨
		
	navigationObj.topFun();
	var infotemplet = $("#docinfotemplettable");
	if(infotemplet.length > 0){
		kmodDiv.find("ul").append("<li><a name=\"docinfotemplettable\" href=\"javascript:void(0)\">ä¿¡æ¯æ¨¡å</a></li>");
	}
	if(kmodDiv.find("li").length == 0){
		kmodDiv.find("ul").append("<li style=\"color: #666666\">ææ¶æ²¡æç¥è¯é­å</li>");
	}
	 //ç»å®é¡µé¢æ»å¨æ¡äºä»¶
	if(IE){
		$(window).scroll(function(){
			if(isIE6){
				navigationObj.topFunForIE6();
			}
			else{
				navigationObj.topFun();  
			}
		});
	}
	else{
		$(document).scroll(function(){
			navigationObj.topFun();  
		});
	}
	//æ¯ä¸¤ç§è°ç¨è¿ä¸ªå½æ°
	var IntervalFun = setInterval(function(){
		navigationObj.objHeight =  $("#place_div").offset().top;
		//navigationObj.objHeight = $(".mainnav-l").offset().top;
	},2000);
	$("a.kmod").parent().hover(function(){
		$(this).removeClass("hide").addClass("wshow")
		$(this).find("div[name=div_fun_kmod]").show();
	},function(){
		$(this).removeClass("wshow").addClass("hide")
		$(this).find("div[name=div_fun_kmod]").hide();
	});
	$("a.pic").click(function(){
		navigationObj.recordLogFun("stat_hdstat_onclick_doc_pic");
		var len = navigationObj.isFloat ? 45:75;
		len = isIE6?(len+10):len;
		navigationObj.topElement.hide();
		$(document).scrollTop($("#figure").offset().top-len);
		return false;
	});

	$("a.discuss").click(function(){
		navigationObj.recordLogFun("stat_hdstat_onclick_doc_discuss");
		navigationObj.topElement.hide();
		$(document).scrollTop($("p.place-bottom").offset().top);
		return false;
	});
	//å¼æ¾åç±»
	/*
	$(".catp").click(function(){
		$("div.mainnav-l").find("li").removeClass("wshow").addClass("hide");
		$("div.mainnav-l").find("div[name='div_fun_kmod']").hide();
		navigationObj.topElement.hide();
		$(document).scrollTop($("#catp_bottom").offset().top-10);
	});
	*/
	kmodDiv.find("a").click(function(){
		navigationObj.scrollFun(this,"widget");
	});
	$("a[name^='hdtop_']").click(function(){
		navigationObj.referenceFun(this,true);
		return false;
	});
	$("a[name^='hdend_']").click(function(){
		navigationObj.referenceFun(this,false);
		return false;
	});
  },
  // å¯¼èªåæ¢æ¾ç°æ§å¶
 topFun : function(){
	$("div.ac_results").hide();  
    var sclTop = $(document).scrollTop();
	if(sclTop >= navigationObj.objHeight){
		// ieæµè§å¨çBUGï¼éè¦åéèç»å¯¹å®ä½çå­åç´ 
		if(IE&&!navigationObj.isFloat){
			navigationObj.topElement.find("div[name=div_fun_kmod]").hide();
			$(".fun_sns_div").hide();
			navigationObj.topElement.find("a.kmod").parent().removeClass("wshow").addClass("hide");
		}
	   if(!navigationObj.isFloat){
		   navigationObj.topElement.hide();
		   navigationObj.floatElement.show();
		   navigationObj.isFloat = true;
	   }
    }
    else{
	   if(navigationObj.isFloat){
		 if(IE){
			navigationObj.floatElement.find("div[name=div_fun_kmod]").hide();
			$(".fun_sns_div").hide();
			navigationObj.floatElement.find("a.kmod").parent().removeClass("wshow").addClass("hide");
		 }	
		 navigationObj.floatElement.hide();
		 navigationObj.topElement.show();
		 navigationObj.isFloat = false;
	  }
   }
  },
  topFunForIE6 : function(){
	  $("div.ac_results").hide();  
	   var sclTop = $(document).scrollTop();
	  if(sclTop > navigationObj.objHeight){		
			// ie6ä¸ä¸ç¨jsæ§å¶
		   var topValue = $("#hd_ad_doc_29").height();
		   if(topValue > 100){
			 navigationObj.primary_top = topValue;
		   } 
		  $("div[name=div_fun_kmod]").hide();
		  $(".fun_sns_div").hide();
		  $("a.kmod").parent().removeClass("wshow").addClass("hide");
		   navigationObj.floatElement.hide();
		   if (navigationObj.timeout){
			   clearTimeout(navigationObj.timeout);
		   }
		   navigationObj.timeout = setTimeout(function(){   
			   navigationObj.floatElement.css("position","absolute");
			   navigationObj.floatElement.css("top",(sclTop - navigationObj.primary_top - navigationObj.headHeight)+'px');
			   if(!navigationObj.isFloat){
				   navigationObj.isFloat = true;
				   navigationObj.topElement.hide();
			   }	   
			   navigationObj.floatElement.show();
		    },200);
	    }
	    else{
	    	  if (navigationObj.timeout){
	 			   clearTimeout(navigationObj.timeout);
	 		  }
	    	 if(navigationObj.isFloat){
				 navigationObj.floatElement.find("div[name=div_fun_kmod]").hide();
				 $(".fun_sns_div").hide();
				 navigationObj.floatElement.find("a.kmod").parent().removeClass("wshow").addClass("hide");
	    		 navigationObj.floatElement.hide();
	    		 navigationObj.topElement.show();
	    		 navigationObj.isFloat = false;
	    	 }
	   }
  },
  // å½çªå£å¤ªå°ï¼ç®å½è¿é¿æ¶ï¼åºç°æ»å¨æ¡
  setElementSize:function(ele){
	  var winHeight = "";
	  if (window.innerHeight) {
		  winHeight = window.innerHeight; 
	  }
	  else if ((document.body) && (document.body.clientHeight)){
		  winHeight = document.body.clientHeight; 
		  if (document.documentElement && document.documentElement.clientHeight && document.documentElement.clientWidth) { 
			  winHeight = document.documentElement.clientHeight; 
			  winWidth = document.documentElement.clientWidth; 
	  	 } 
	  } 
// var wintop = ele.offset().top;
	ele = ele.find("dl");
	var e_height  = ele.length > 0 ?ele.get(0).offsetHeight:0;
	winHeight = winHeight*0.9 ;
	if(winHeight < e_height){
		if(isIE6){
			ele.css("height",winHeight);
		}
		else{
			ele.css("max-height",winHeight);
		}
		ele.css("overflow-y","scroll");
		ele.css("overflow-x","hidden");
	}
	else{
		ele.removeAttr('style');
	}
  },
  recordLogFun : function(watchflag){
		try {
			 StatVirtualTraffic(document.referrer, window.location, watchflag);
		} 
		catch (e) {
		}
	},
 // ç¹å»å¯¼èªä¸çç®å½æèç¥è¯æ¨¡åè·³å°å¯¹åºæ¨¡æ¿
  scrollFun : function(ele,type){
	var eleName = ele.name;	
	var muluObj = "";
	var recordName ="";
	if(type=="widget"){
		muluObj = eleName == "docinfotemplettable"?$("#"+eleName):$("div[appcode="+eleName+"]");
		recordName = "stat_hdstat_onclick_doc_kmod"
	}
	else{
		eleName = eleName.substring(eleName.indexOf("a_fun_")+6);
		muluObj = $("a[name="+eleName+"]");
		recordName = "stat_hdstat_onclick_doc_catalog"
	}
	navigationObj.recordLogFun(recordName);
	if(muluObj.length > 0){
		muluObj = muluObj.get(0);
		var len = navigationObj.isFloat ? 60:90;
		$(document).scrollTop($(muluObj).offset().top-len);
		//$("div[name=div_fun_catalog]").hide();
		$("div[name=div_fun_kmod]").hide();
		return false;
	}
	return false;
  },
  // åèèµæ
  referenceFun: function(ele,flag){
	var	eleName = $(ele).attr("href").substring(1);
	var	muluObj = $("a[name="+eleName+"]");
	if(muluObj.length > 0){
		muluObj = muluObj.get(0);
		if(flag){
		    $("a[name^='hdend_']").removeClass("reference_high");
			$(muluObj).addClass("reference_high");
		}
		var len = navigationObj.isFloat ? 60:90;
		$(document).scrollTop($(muluObj).offset().top-len);
		return false;
	}
	return false;
  }
  
}

function strLen(str){  
    return str.replace(/[^\x00-\xff]/g,"xx").length;  
}

var editPowerFlag = false;	  
//ç¼è¾æç¤º
function editOK(sectionid){
	var doctitle = document.getElementById("doctitle").value;
	var _docTitle = checkURIValue(doctitle);

	if($('#RadioGroup1_0')[0].checked){
		window.location = 'http://www.baike.com/wikdoc/sp/editDocAuth.do?doc_title='+_docTitle+'&islostdraft=1';
	}else{
		if(editPowerFlag){
			postAsForm("http://www.baike.com/wikdoc/sp/eidtdocRequest.do?doc_title=" + _docTitle);
		}
		else{
			if (typeof(sectionid) == 'undefined' || null == sectionid || "" == sectionid) {
				window.location = 'http://www.baike.com/editdocauth/'+_docTitle;
			}else if (/[1-9]+[0-9]*/.test(sectionid)) {
				window.location = 'http://www.baike.com/editsectionauth/'+_docTitle+'/'+sectionid;
			}
		}
	}
}		

function closeWindow(){
	$.dialog.close('docEditAsk');
	return false;
} 
		 
function  polysemycheck(){
		var doctitle_polysemy_value = document.getElementById('doctitle_polysemy').value;
		
		//var doc_title=doctitle_polysemy_value+"&polysemy=true";
		var doc_title=doctitle_polysemy_value+"&prd=polysemy";
		var doc_url="http://www.baike.com/sp/wiki/"+doc_title;
		//var doc_url="http://wikdoc.hudong.com/entryview.do?doc_title="+doc_title;
		doc_url=encodeURI(doc_url);
		window.location.href=doc_url;
	}

//å³æ³¨ add by yangzaizhong
followRelation = function(flag, userIden, object) {
//	if (!SnsCommon.isLogin()) {
//		$("#follow_prompt").html("æªç»å½ï¼");
//		return;
//	}
	
	var url = "http://i.baike.com/ajaxChangeFollowRelation.do?t=" + Math.random() + "&relationFlag=" + flag + "&followUserIdEn=" + userIden + "&callback=?";
	if (flag == 1) {
		$("#follow_detail").html('<span class="red" id="follow_prompt"></span><span class="btn-ingz">å³æ³¨</span>');
	}else if(flag == 0){
		$("#follow_detail").html('<span class="red" id="follow_prompt"></span><span class="btn-ingz">åæ¶</span>');
	}
	
	$.getJSON(url, function(jsondata){
		if (jsondata != null) {
			if (jsondata.followInfo == "è¯·ç»å½") {
				var date = new Date();
				date.setTime(date.getTime() + 24 * 3600 * 1000);
				document.cookie = 'nextURL=' + document.location.href + ';'
						+ 'expire=' + date.toGMTString()
						+ ';path=/;domain=.baike.com;';
				window.location.href = "http://wiki.baike.com/user/login/userLogin.jsp";
				return;
			}
			if (jsondata.followflag == 1) {
				if (flag == 1) {
					$("#follow_detail").html('<span class="red" id="follow_prompt"></span><span class="btn-yigz">å·²å³æ³¨<a  onclick="followRelation(0,\'' + userIden + '\',this);return false;" href="javascript:void(0)" >åæ¶</a></span>');
				} else {
					$("#follow_detail").html('<span class="red" id="follow_prompt"></span><a href="javascript:void(0)" class="btn-togz" onclick="followRelation(1,\'' + userIden + '\',this);return false;">å³æ³¨</a>');
				}
				//å é¤ç¼å­
				MomoForVEditor.cache_list[userIden] = null;
			} else {
				$("#follow_prompt").html("æä½å¤±è´¥ï¼");
			}
		} else {
			$("#follow_prompt").html("æä½å¤±è´¥ï¼");
		}
	});
}

subStr = function(str,length){  
    var a=str.match(/[^\x00-\xff]|\w{1,2}/g);  
    return a.length<length?str:a.slice(0,length).join("")+"...";  
}

subStrTitle = function(str,length){  
    var a=str.match(/[^\x00-\xff]|\w{1,2}/g);  
    return a.length<length?str:a.slice(0,length).join("");
}



//é¡µé¢å±å¼æ¶èµ·
var spread={
	
	init:function(){
		var self = this;
		self.setTagShow();
		self.openTag(); 
		self.closeTag();
		self.setThesaurusShow();
		self.openThesaurus();
		self.closeThesaurus();
		self.openPolysemant();
		self.closePolysemant();
		
	},
	
	//å¼æ¾åç±»æ ç­¾åºåå±ç¤ºæ ·å¼è®¾ç½®
	setTagShow: function() {
	    var self = this;
	    var tagArr = $("#show_tag").find("dd").find("a");
	
	    var tagLen = 0;
	    //å¼æ¾åç±»æ ç­¾ä¹é´é´è·
	    var aInterval = 4;
	    //å¼æ¾åç±»æ ç­¾åé¨ä¸¤è¾¹è¾¹æ¡é´è·å
	    var aInnerInterva = 14;
	    //è®¡ç®å¼æ¾åç±»æ ç­¾æ»å®½åº¦
	    for (var i = 0; i < tagArr.length; i++) {
	     	  var tagTxt = $(tagArr[i]).text();
	     	  var tCount = HD_getLength(tagTxt); 
	        tagLen = tagLen + (tCount * 6) + aInnerInterva + aInterval; //æ¯ä¸ªè±æå­ç¬¦å®½åº¦ä¸º6ï¼ä¸­æå®½åº¦ç¿»å
	    }
	
	    //å°å¤å çé´è·åæ
	    if (tagLen > 0) {
	        tagLen = tagLen - aInterval;
	    }
	    //782ä¸ºæ­£æè¯æ¡é¡µæ­£æå®½åº¦	
	    if (tagLen > 782) {
	        $("#close_tag").hide();
	        $("#open_tag").show();
	    } else {
	        $("#open_tag").hide();
	        $("#close_tag").hide();
	
	    }
	},
	
	//åä¹è¯æ ç­¾åºåå±ç¤ºæ ·å¼è®¾ç½®
	setThesaurusShow: function() {
	    var self = this;
	    var thesaurusArr = $("#show_thesaurus").find("dd").find("a");
	    var thesaurusLen = 0;
	    //åä¹è¯æ ç­¾ä¹é´é´è·
	    var aInterval = 4;
	    //åä¹è¯æ ç­¾åé¨ä¸¤è¾¹è¾¹æ¡é´è·å
	    var aInnerInterva = 14;
	    //è®¡ç®åä¹è¯æ ç­¾æ»å®½åº¦
		  for (var i = 0; i < thesaurusArr.length; i++) {
		    var thesaurusTxt = $(thesaurusArr[i]).text();
		    var tCount = HD_getLength(thesaurusTxt); 
		    thesaurusLen = thesaurusLen + (tCount * 6) + aInnerInterva + aInterval; //æ¯ä¸ªè±æå­ç¬¦å®½åº¦ä¸º6ï¼ä¸­æå®½åº¦ç¿»å
	    }
	
	    //å°å¤å çé´è·åæ
	    if (thesaurusLen > 0) {
	        thesaurusLen = thesaurusLen - aInterval;
	    }
	
	    if (thesaurusLen > 782) {
	        $("#close_thesaurus").hide();
	        $("#open_thesaurus").show();
	    } else {
	        $("#open_thesaurus").hide();
	        $("#close_thesaurus").hide();
	    }
	},
	
	//å¼æ¾åç±»
	openTag : function(){
		var self = this;
		$("#open_tag").bind("click",function(){
			$("#open_tag").hide();
			$("#close_tag").show();
			$("#show_tag").find("dd").removeAttr("class");
		});
		$("#close_tag").bind("",function(){
			$("#close_tag").hide();
			$("#open_tag").show();
		  $("#show_tag").find("dd").attr("class","h27");
		});
	},
	
	closeTag : function(){
		var self = this;
		$("#close_tag").bind("click",function(){
			$("#close_tag").hide();
			$("#open_tag").show();
			$("#show_tag").find("dd").attr("class","h27");
		});
	},
	//åä¹è¯
	openThesaurus : function(){
		var self = this;
		$("#open_thesaurus").bind("click",function(){
			$("#open_thesaurus").hide();
			$("#close_thesaurus").show();
			$("#show_thesaurus").find("dd").removeAttr("class");
		});
	},
	closeThesaurus : function(){
		var self = this;
		$("#close_thesaurus").bind("click",function(){
			$("#close_thesaurus").hide();
			$("#open_thesaurus").show();
		    $("#show_thesaurus").find("dd").attr("class","h27");	
		});
	},
	//å¤ä¹è¯
	openPolysemant : function(){
		var self = this;
		$("#open_polysemant").bind("click",function(){
			$("#open_polysemant").hide();
			$("#close_polysemant").show();
			$("#polysemyAll").removeAttr("class");
		});
	},
	closePolysemant : function(){
		var self = this;
		$("#close_polysemant").bind("click",function(){
			$("#close_polysemant").hide();
			$("#open_polysemant").show();
			$("#polysemyAll").attr("class","h55");
		});
	}

};
//ä¿®æ¹è¯æ¡é¡µæ­£æåé¾
var innerLink={
	init:function(){
		var self=this;
		self.modify();
	},
	modify:function(){
		$("#content").find("a[class='innerlink']").each(function(i){
			var href=$(this).attr("href");
			href=href.replace("wiki","sowiki");
			if(href && href.indexOf('www.baike.com/wiki')){
				href=href+'?prd=content_doc_search';
				$(this).attr("href",href);
			}
		});
	}
};
//è¯æ¡é¡µå¾çå å¾å
var imageOperation={
	init:function(){
		var self=this;
		//self.right();
		//self.left();
		self.allModify();
	},
	
	allModify:function(){
		var doctitle = $("#doctitle").val();
		$('#content div').not($("table")).find('img').each(function(i){
			var imgUrl = $(this).attr('data-original');
			if(imgUrl && imgUrl.indexOf('att.hudong.com') > 0){
				var parentTag=$(this).parent('a');
				var href=parentTag.attr("href");				
				if(href){
					var patrn=/http:\/\/tupian\.baike\.com\/(a|b)/;
					var newhref=href;
					if(patrn.exec(href)){
						newhref=href.replace("tupian.baike.com/","tupian.baike.com/doc/"+doctitle+"/");						
					}					
					parentTag.attr("href",newhref+"?prd=citiao_tuce_zhengwen");
					var text=parentTag.parent('div').find("strong").text();
					if(text.length<=0)return true;
					var html=text+'<a href="'+newhref+"?prd=citiao_tuce_zhengwen"+'" target="_blank"><em class="icard"></em>å¾å</a>';
					parentTag.parent('div').find("strong").html(html);
				}else{
					var reg = "http://(a0|a1|a2|a3|a4).(att).(hudong|hoodong).com/(\\d*)/(\\d*)/(\\d*)(.*)(\\.)(gif|jpg|jpeg|png|bmp)";
					var specs = imgUrl.match(reg);
					var div = $(this).parent('div');
					var imgHtml = '<img id= "'+$(this).attr('id')+'" data-original="'+$(this).attr('data-original')+'" src="'+$(this).attr('data-original')+'" alt="'+doctitle+'" style="'+$(this).attr('style')+'"/>';
					var txt = div.find("strong").text();
					var href = 'http://tupian.baike.com/doc/test/'+specs[1]+'_'+specs[4]+'_'+specs[5]+'_'+specs[6]+'_'+specs[9]+'.html?prd=citiao_tuce_zhengwen';
					var html = '<a target="_blank" href= "'+href+ '" title="'+txt+'">';
					html += imgHtml +'</a>';
					html += '<strong>'+txt+'<a target="_blank" href= "'+href+'"><em class="icard"></em>å¾å</a></strong>';
					div.children('img').remove();
					div.children('strong').remove();
					div.html(html);
				}
				
			}
		});
	},	
	right:function(){
		$(".img_r").each(function(i){
			var href=$(this).find("a").attr("href");
			if(href){
				$(this).find("a").attr("href",href+"?prd=citiao_tuce_zhengwen");
				var text=$(this).find("strong").text();
				var html=text+'<a href="'+href+"?prd=citiao_tuce_zhengwen"+'" target="_blank"><em class="icard"></em>å¾å</a>';
				$(this).find("strong").html(html);
			}
		});
	},
	left:function(){
		$(".img_l").each(function(i){
			var href=$(this).find("a").attr("href");
			if(href){
				$(this).find("a").attr("href",href+"?prd=citiao_tuce_zhengwen");
				var text=$(this).find("strong").text();
				var html=text+'<a href="'+href+"?prd=citiao_tuce_zhengwen"+'" target="_blank"><em class="icard"></em>å¾å</a>';
				$(this).find("strong").html(html);
			}
		});
	}
};

