			var error = '<div class="docpagepopwindow"><dl><dt class="a-c"><span class="add_fl">' + returnhtml + '</span></dt><dd class="a-c"><input name="Button2" type="button" value="ç¡® å®" onclick="HdDialog.closeLoadingDialog();" class="btnOK" /></dd></dl></div>';
