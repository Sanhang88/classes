	try {
		GUnload();
	} 
	catch (e) {
	}
});


function changeTo(num,ulId,spanId,liNum,pageSize,liLen){ 
	if((num+1) != pageSize){
		 var goLeft = num * 782; 
	   var liLenTemp = 0; 
	   for(var i = 0;i<liNum;i++){	
	     	  $li =  $("#"+ulId +" li")[i];
	     	  liLenTemp = liLenTemp + $li.offsetWidth + liInterval;
	     	  if(goLeft < (liLenTemp - liInterval)){
	     	  	goLeft  = liLenTemp - liInterval - $li.offsetWidth;
	     	  	break;
	     	  }
	    }   
	    perGoLeft  = goLeft;
	}else{
		 //ä»ä»ç§»å¨å©ä½çé¿åº¦
		 var remainLen = liLen - (perGoLeft + 782);
		 goLeft = perGoLeft + remainLen;
	}
  
    $("#"+ulId).animate({left: "-" + goLeft + "px"},500);

    var l = setInterval(function(){ 
	      clearInterval(l)
        $("#"+spanId).text(num+1);
	  }, 500);
}

/**
 * å°è£ææçå¼¹åºæ¶æ¯ï¼ä¾¿äºç»ä¸å¼¹çª
 * 
 * @param {Object}
 *            data
 */
function HDAlert(data, autoCloseTime){
	data = '<div class="info_box"><p>'+data+'</p></div>';
	$.dialog({
		id: 'snsDialog',
		overlay: false,
		autoClose: typeof(autoCloseTime) == 'undefined' ? 2000 : autoCloseTime,
		title: 'æç¤º',
		// effects: 'fade',
		height: 100,
		width: 230,
		content: data,
    styleContent : {
				textAlign : 'center'
		},
		skin: "bluebox"
	});
}

/**
 * å·¥å·å¯¹è±¡,åæ¬ææçå¬å±æ¹æ³ã
 */
var commonTool = {
	// å¤æ­dataä¸­æ¯å¦å«æhtmlæ ç­¾
	checkHTML: function(data){
		var reg = /<(.*)>.*<\/\1>/;
		if (data.search(reg) > -1) {
			return true;
		}
		return false;
	},
	// æ¿æ¢
	replaceAll: function(data, s1, s2){
		return data.replace(new RegExp(s1, "g"), s2);
	},
	// è·åcookie
	getCookie: function(name){
		var start = document.cookie.indexOf(name + "=");
		var len = start + name.length + 1;
		if ((!start) && (name != document.cookie.substring(0, name.length))) {
			return null;
		}
		if (start == -1) {
			return null;
		}
		var end = document.cookie.indexOf(';', len);
		if (end == -1) 
			end = document.cookie.length;
		return unescape(document.cookie.substring(len, end));
	},
	// å¤æ­äºå¨ç¨æ·æ¯å¦ç»å½
	isLogin: function(){
		return (commonTool.getCookie("hduser") != null);
	},
	getReplyTime: function(){
		var time = new Date();
		var year = time.getFullYear();
		var month = (time.getMonth() + 1) < 10 ? "0" + (time.getMonth() + 1) : time.getMonth() + 1;
		var date = time.getDate() < 10 ? "0" + time.getDate() : time.getDate();
		var h = time.getHours() < 10 ? "0" + time.getHours() : time.getHours();
		var m = time.getMinutes() < 10 ? "0" + time.getMinutes() : time.getMinutes();
		return (year + "-" + month + "-" + date + " " + h + ":" + m);
	},
	filterA: function(data){
		return data.replace(/<a[^>]*>([^<]*)<\/a>/ig, '$1');
	}
};
/**
 * iframeèªéåºé«åº¦
 * 
 * @param F
 * @param autoH
 * @param autoW
 * @return
 */
function autoHeight(F, autoH, autoW){
	var height = 20, width = 30, dom;
	if ($.browser.msie) {
		dom = F.Document.body;
	}
	else {
		dom = F.contentDocument.body;
	}
	if (autoH) {
		height += dom.scrollHeight;
		if ($.browser.mozilla) {
			// height += 10;
		}
		F.style.height = dom.scrollHeight + 'px';
	}
	if (autoW) {
		width += dom.scrollWidth;
		F.style.width = width + 'px';
	}
}

/**
 * è®°å½æ¥å¿ï¼ç¨äºè¡ä¸ºç»è®¡ï¼å©ç¨æ´çªçhttp://stat.baike.com/js/webstat_manage.jså®ç°
 */
function recordLog(watchflag){
	try {
		StatVirtualTraffic(document.referrer, window.location, watchflag);
	} 
	catch (e) {
	}
}

