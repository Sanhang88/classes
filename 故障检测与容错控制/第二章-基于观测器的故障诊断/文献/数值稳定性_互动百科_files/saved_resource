		docInfoTemplet.init(templet_title);
	});
	return false;
}
/**
 * è¯æ¡é¡µé¢ï¼åæ¬åä¹è¯ï¼ajax
 */
function getdocinfo(){
	var doctitle = $("#doctitle").val();
	var docid = $("#docid").val();
	var dociden = $("#dociden").val();
	var url = "/getdocinfo.do?docid=" + docid + "&dociden=" + dociden + "&doctitle=" + encodeURIComponent(doctitle);
	if ($("#creatoriden").length > 0) {
		url += "&creatoriden=" + $("#creatoriden").val();
	}
	$.ajax({
		dataType: 'xml',
		type: 'GET',
		url: url,
		success: function(xml){
			var userAuditstate = $(xml).find("userAuditstate");
			var docHisFlag = $(xml).find("docHisFlag");
			if(docHisFlag.length > 0&&docHisFlag.text()=="1"){
				$('.content-h1 h1:first').text("æªå®¡æ ¸è¯æ¡");
			}
			var creatorLevelTitle = $(xml).find("LEVELTITLE");
			if (creatorLevelTitle.length > 0) {
				$("#creatorli").append(creatorLevelTitle.text());
			}
			var missionPackage = $(xml).find("missionPackage");
			if(missionPackage.length>0){
				var url = missionPackage.find("url");
				var description = missionPackage.find("description");
				if(url.length>0&&description.length>0){
					$('#doc_mission_view').html('<p><a href="'+url.text()+'" target="_blank">'+description.text()+'</a></p>');
				}
			}
			//ä¿®æ¹å¾çæ°é
			var imgNumber = $(xml).find("picCount");
			var nav_a_pic = $("a.pic");
			if(imgNumber.length>0){
				var imgNumber = imgNumber.text();
				if(imgNumber>0){
					imgNumber = imgNumber == 4?"4+":imgNumber;
					nav_a_pic.append("<span>"+imgNumber+"</span>");
				}else{
					nav_a_pic.append("<span>0</span>");
				}
			}else{
				nav_a_pic.append("<span>0</span>");
			}
			
			var loginstate = $(xml).find("loginstate");
			if (loginstate.length > 0) {
				var adminstate = loginstate.find("adminstate").text();
				var addmapauth = loginstate.find("addmapAuth").text();
				var tongyici = loginstate.find("tongyici").text();
				var doctag = loginstate.find("doctag").text();
				var xgct = loginstate.find("xgct").text();
				if (tongyici == "1") {
					$("#show_thesaurus_dt").append('<a id="addthesaurus" href="javascript:newaddthesaurus();" class="r">ææ¥è¡¥å</a>').parent().show();
					$("#thesaurusADD").show();
				}
				if (doctag == "0") {
					$('#show_tag a.completetaghref').hide();
				}
				if (xgct == "1") {
					
					$("#a_xgct").show();
				}
				// å¤æ­æ¯å¦æ¯æ­£æé¡µ
				if ($('#pagetype').val() == 'doc') {
					if (adminstate == "1") {
						$('#editmore').removeAttr('style');
						$('#editmore').append('<a href="javascript:void(0)" id="renamehref">å¤ä¹è¯</a>');
						$('#renamehref').bind("click", function(){
							HdTools.loadJs(loadviewdocfunctionjsurl,function(){
								loadviewdocfunctionjsflag = true;
								ShowDocRename();
							});
							return false;
						});
					}
					if (addmapauth == '1') {
						appInfo.mapeditauth = true;
					}
				}
				if ($('#_loginstate').length > 0) {
					$('#_loginstate').val(1);
				}
			}
			else {
				$('_doc_flag').val('/wiki/' + $("#doctitle_utf8").val());
				if ($("#topic_email_li").length > 0) {
					$("#topic_email_li").show();
				}
				if ($("#topic_pwd_li").length > 0) {
					$("#topic_pwd_li").show();
				}
			}
			if (getCookie('hduser') == null) {
				$("#submitTopicBtn").val("ç»éå¹¶åå¸");
			}
			if ($('#unifyprompt').length > 0) {
				var editlock = $(xml).find("editlock");
				if (editlock.length > 0) {
					var content = '<p id="unifypromptthree">æ­¤è¯æ¡æ­£å¨è¢«';
					var lockuserid = editlock.find("userid").text();
					if (lockuserid == "0") {
						var arrIP = editlock.find('userip').text().split(".");
						var userIP = arrIP[0] + "." + arrIP[1] + "." + arrIP[2] + ".*";
						content += "å¿åç¨æ·(" + userIP + ")éå®ç¼è¾ä¸­!";
					}
					else {
						content += "<a href='http://weiji.baike.com/u/" + editlock.find('useriden').text() + "/index'>" + editlock.find('usernick').text() + "</a>éå®ç¼è¾ä¸­!</p>";
					}
					$('#unifyprompt').prepend(content).show();
				}
				else {
					if ($(xml).find('overTwoLocks').length > 0) {
						$('#unifyprompt').append("<p>æ¨å·²ç»éå®äºä¸¤ä¸ªä»¥ä¸çè¯æ¡ï¼æä»¥æ¨ä¸è½å¤ç»§ç»­ç¼è¾è¯¥è¯æ¡</p>").show();
					}
				}
				
				if ($('#unifyprompt').length > 0) {
					if ($.trim($('#unifyprompt').html()) == '') {
						$('#unifyprompt').remove();
					}
					else {
						if ($('#unifypromptone').length > 0 && $('#unifyprompttwo').length > 0) {
							$('#unifyprompttwo').addClass('line');
						}
						else 
							if ($('#unifypromptone').length > 0 && $('#unifypromptthree').length > 0 || $('#unifyprompttwo').length > 0 && $('#unifypromptthree').length > 0) {
								$('#unifypromptthree').addClass('line');
							}
							else 
								if ($('#unifypromptone').length > 0 && $('#unifyprompttwo').length > 0 && $('#unifypromptthree').length > 0) {
									$('#unifyprompttwo').addClass('line');
								}
						$('#unifyprompt').show();
					}
				}
			}	
			// é¡¶çæ¬¡æ°æ°
			// if ($(".recommendhref").length > 0) {
			// $('.recommendhref').text('é¡¶[' +
			// $(xml).find('RecommendNum').text() + ']');
			// }
			if ($(".digg").length > 0) {
				$('.digg').text($(xml).find('RecommendNum').text());
			}
			// ä¸ä¼ éå¾ï¼ç«å¡å¤æ­
			var networkerstate = $(xml).find('networkerstate').text();
			if(typeof(networkerstate) != "undefined" && networkerstate == 1){
				if($('#addFigure').length > 0){
					$('#addFigure').show();
				}
				
				 //æ ¹æ®"ä¸ä¼ å¾ç"å"æ´å¤"ä¸¤ä¸ªè¶é¾æ¥çç¶æç¡®å®ä¸­é´æ¯å¦æåå²ç¬¦
			   if($('#addFigure').length > 0 && $("#addFigure").is(':visible') && $("#moreId").length > 0){
			    	if ($('#kongGeId').length <= 0){
			    		var html = '<em id="kongGeId">&nbsp;|&nbsp;</em>';
			    	  $("'"+html+"'").insertAfter($("#addFigure"));
			    	}
			   }else{
			      if($('#kongGeId').length > 0){
			      	removeNode(document.getElementById("kongGeId"));
			      }
			   }
			}
		},
		error: function(){
			$('#viewnum').hide();
			$('#topicNum').hide();
		}
	});
}

//æå»ºè®®
function suggestion(){
		// æè¦æå»ºè®®
		if (loaddocquizflag) {
			docquiz();
		}
		else {
			jQuery.getScript("/js/docquiz20120329.js", function(){
				docquiz();
				loaddocquizflag = true;
			});
		}
}
/**
 * ç¼è¾ï¼æ·»å æè¦
 */
function editSummary(){
		var jsUrl = "http://www.huimg.cn/entry/js/docsummary/docsummary170803.js";
	HdTools.loadJs(jsUrl, function(){
		var doctitle = $('#doctitle').length > 0 ? $('#doctitle').val() : "";
		DocSummaryEdit.tflag = null;
		DocSummaryEdit.requestEdit(doctitle);
	});
	return false;
}


/**f
 * æ¶æ­§ä¹è·åç¨æ·æé æ¯å¦æ¾ç¤ºâæ·»å âåâç¼è¾âé¾æ¥
 */
function getAmbiguityInfo(){
	var doctitle = $('#doctitle').val();
	var url = "/ambiguityDeal.do?Action=getPopedomInfo&doctitle=" + encodeURI(doctitle);
	$.ajax({
		dataType: 'xml',
		type: 'get',
		url: url,
		timeout: 4000,
		success: function(xml){
			$('#viewnum').html('éè¯»(' + $(xml).find("viewnum").text() + ')');
			var stateinfo = $(xml).find("STATEINFO").text();
			var ambid_str = '';
			if (stateinfo == "1") {
				ambid_str = '<li><a href="#" id="ambiguityaddhref" >æ·»å å¤ä¹è¯</a></li>' +
				'<li><a href="#" id="ambiguityedithref">ç¼è¾å¤ä¹è¯</a></li>';
			}
			if (stateinfo == "2") {
				ambid_str = '<li><a href="#" id="ambiguityaddhref">æ·»å å¤ä¹è¯</a></li>';
			}
			// è®¨è®ºåº,åä¸è®¨è®ºæ¾ç¤ºå¯å§ä¸»é¢ååå¸æ°é
			var topicNum = $(xml).find('TopicNum');
			if (topicNum.length > 0) {
				if ($('#topichref').length > 0) {
					$('#topichref').text('è®¨è®º(' + topicNum.text() + ')');
				}
			}
			$('#operate').append(ambid_str);
			$('#ambiguityaddhref').click(function(){
				if (loadambiguity0909flag) {
					addAmbigDiv(1);
					return false;
				}
				else {
					jQuery.getScript("/js/ambiguity0909.js", function(){
						addAmbigDiv(1);
						loadambiguity0909flag = true;
						return false;
					});
				}
			});
			$('#ambiguityedithref').click(function(){
				if (loadambiguity0909flag) {
					addAmbigDiv(2);
					return false;
				}
				else {
					jQuery.getScript("/js/ambiguity0909.js", function(){
						addAmbigDiv(2);
						loadambiguity0909flag = true;
						return false;
					});
				}
			});
		},
		error: function(){
			$('#viewnum').html('éè¯»');
		}
	});
}
/*
 * // åå°é¡¶é¨ by dly var scrolltotop = { setting: { startline: 100, scrollto: 0,
 * scrollduration: 1000, fadeduration: [500, 100] }, controlHTML: '<img
 * src="http://www.huimg.cn/wikispace/images/up.png" style="width:23px;
 * height:66px" />', // HTML // for // control, // which // is // auto //
 * wrapped // in // DIV // w/ // ID="topcontrol" controlattrs: { offsetx: 30,
 * offsety: 80 }, // offset of control relative to right/ bottom of window
 * corner anchorkeyword: '#top', // Enter href value of HTML anchors on the page
 * that // should also act as "Scroll Up" links state: { isvisible: false,
 * shouldvisible: false }, scrollup: function(){ if (!this.cssfixedsupport) { //
 * if control is positioned using // JavaScript this.$control.css({ opacity: 0
 * }); // hide control immediately after clicking it } var dest =
 * isNaN(this.setting.scrollto) ? this.setting.scrollto :
 * parseInt(this.setting.scrollto); if (typeof dest == "string" && jQuery('#' +
 * dest).length == 1) {// check // element // set // by // string // exists dest =
 * jQuery('#' + dest).offset().top; } else { dest = 0; } this.$body.animate({
 * scrollTop: dest }, this.setting.scrollduration); }, keepfixed: function(){
 * var $window = jQuery(window); var $documentBody = jQuery(document.body); var
 * controlx = $documentBody.scrollLeft() + $documentBody.width() +
 * ($window.width() - $documentBody.width()) / 2 - this.$control.width() -
 * this.controlattrs.offsetx; var controly = $window.scrollTop() +
 * $window.height() - this.$control.height() - this.controlattrs.offsety;
 * this.$control.css({ left: controlx + 'px', top: controly + 'px' }); },
 * togglecontrol: function(){ var scrolltop = jQuery(window).scrollTop(); if
 * (!this.cssfixedsupport) { this.keepfixed(); } this.state.shouldvisible =
 * (scrolltop >= this.setting.startline) ? true : false; if
 * (this.state.shouldvisible && !this.state.isvisible) {
 * this.$control.stop().animate({ opacity: 1 }, this.setting.fadeduration[0]);
 * this.state.isvisible = true; } else { if (this.state.shouldvisible == false &&
 * this.state.isvisible) { this.$control.stop().animate({ opacity: 0 },
 * this.setting.fadeduration[1]); this.state.isvisible = false; } } }, init:
 * function(){ jQuery(document).ready(function($){ var mainobj = scrolltotop;
 * var iebrws = document.all; mainobj.cssfixedsupport = !iebrws || iebrws &&
 * document.compatMode == "CSS1Compat" && window.XMLHttpRequest; // not // IE //
 * or // IE7+ // browsers // in // standards // mode mainobj.$body =
 * (window.opera) ? (document.compatMode == "CSS1Compat" ? $('html') :
 * $('body')) : $('html,body'); mainobj.$control = $('<div id="topcontrol"
 * style="z-index:11;">' + mainobj.controlHTML + '</div>').css({ position:
 * mainobj.cssfixedsupport ? 'fixed' : 'absolute', bottom:
 * mainobj.controlattrs.offsety, right: mainobj.controlattrs.offsetx, opacity:
 * 0, cursor: 'pointer' }).attr({ title: 'åé¡¶é¨' }).click(function(){
 * mainobj.scrollup(); return false; }).appendTo('body'); if (document.all &&
 * !window.XMLHttpRequest && mainobj.$control.text() != '') {// loose // check //
 * for // IE6 // and // below, // plus // whether // control // contains // any //
 * text mainobj.$control.css({ width: mainobj.$control.width() });// IE6- seems
 * to require an explicit width on a DIV // containing text }
 * mainobj.togglecontrol(); $('a[href="' + mainobj.anchorkeyword +
 * '"]').click(function(){ mainobj.scrollup(); return false });
 * $(window).bind('scroll resize', function(e){ mainobj.togglecontrol(); }); }) } }
 */
/*
 * Momo @required jQuery @ site http://www.baike.com @ version 1.0 @ date
 * 2009.09.09 14:40 @ author: panxuepeng
 */
var Momo = {
	// æ¯å¦å¯ä»¥ä½¿ç¨
	is_active: 1,
	
	// ææè·åè¿æ°æ®çç¼å­ä¿¡æ¯ï¼é¿åéå¤é¾æ¥
	cache_list: [],
	
	// æµè¯æ°æ®çurlï¼å¦ææ¯æ­£å¼ä½¿ç¨è¯·ç½®ç©º
	test_url: '',
	
	// è·åæ°æ®çURL
	url: '/wikdoc/sp/validateDocSummary.do',
	
	// é¼ æ æ¸å°ç <a> æ ç­¾å¯¹è±¡
	a_mouseover: null,
	
	// æ¾ç¤ºå»¶è¿æ è¯ï¼ç¨äºè®°å½setTimeoutçè¿åå¼
	defer_show: 0,
	
	// éèå»¶è¿æ è¯ï¼ç¨äºè®°å½setTimeoutçè¿åå¼
	defer_hide: 0,
	
	// è§¦æ¸å»¶è¿ï¼å»¶è¿æ­¤æ¶é´åè§¦åæ¾ç¤ºäºä»¶
	timeout: 500,
	
	/*
	 * æ¾ç¤ºæ¡divççidï¼å½åçç¨åºä¾èµæ¨¡æ¿çæ¾ç¤ºæ¡çè®¾è®¡ï¼å¦ä¸ï¼ <div
	 * style="width:290px;overflow:hidden;z-index:10; background-color:#d1e7fc;
	 * position:absolute; padding:5px; font-size:12px;
	 * ine-height:20px;top:0;left:0;" id="popup">
	 * <p style="padding:0 0 5px 0; width:290px; margin:0; float:left;" id="headp"/>
	 * <div style="float:left; padding:5px;
	 * width:280px;background-color:#fff;line-height:18px"
	 * id="summaryDataBody"/> </div>
	 */
	id: '#popup',
	
	// æ¯å¦å¯ä»¥é¼ æ æå¨
	movable: true,
	
	// æ¯å¦å¤äºæå¨å½ä¸­
	draging: false,
	
	// æ¯å¦è¢«æå¨äº
	is_draged: false,
	
	// æ¯å¦å¤äºæ¾ç¤ºç¶æ
	is_show: false,
	
	// æ¾ç¤ºå è½½è¯æ¡æè¦è¿ç¨çå¨æå¾ç
	loading: 'http://www.huimg.cn/public/loading.gif',
	
	// æ­£å¨å è½½
	onloading: false,
	
	win: null,
	dom: null,
	div: null,
	
	// çªå£æ»å¨æ¶éè
	scroll_hide: false,
	
	// è·åæç¤ºæ¡çæä½³æ¾ç¤ºåæ 
	getPos: function(E){
		var self = this;
		var left, top, a = $(E), div = self.div, dom = self.dom, win = self.win;
		var dW = div.width(), dH = div.height();
		var pos = a.offset(), aH = a.height(), aW = a.width();
		var cW = win.width(), cH = win.height(), sL = dom.scrollLeft(), sT = dom.scrollTop();
		var body_left = (document.documentElement.clientWidth - document.body.clientWidth) / 2;
		// ä¸é¢ç 0.8 ç­æ¯ä¸ºäºè°æ´å½åé¾ä½ç½®åé¡µé¢çªå£å³ä¾§æ¶ï¼å¨åé¾çå·¦ä¾§æ¹åæ¾ç¤ºæç¤ºæ¡
		var isW = pos.left + dW < cW * 0.8 + sL;
		
		// ä¸é¢ç 0.9 ç­æ¯ä¸ºäºè°æ´å½åé¾ä½ç½®åé¡µé¢çªå£ä¸ä¾§æ¶ï¼å¨åé¾çä¸ä¾§æ¹åæ¾ç¤ºæç¤ºæ¡
		var isH = pos.top + dH < cH * 0.9 + sT;
		left = isW ? pos.left : pos.left - dW + aW;
		top = isH ? pos.top + aH + 2 : pos.top - dH - aH;
		
		left = left - body_left;
		return {
			left: left,
			top: top
		};
	},
	// è·åè¯æ¡çæè¦ä¿¡æ¯
	getData: function(E){
		var self = this;
		var title = $(E).text();
		var doctitle = (title.length > 10) ? (title.substring(0, 10) + "...") : title;
		$("#headp a").html(doctitle).attr('href', 'http://www.baike.com/wiki/' + encodeURI(title));
		
		if (self.cache_list[title]) {
			self.setData(self.cache_list[title]);
			self.show(self.getPos(E));
		}
		else {
			var url = self.url + "?doc_title=" + encodeURI(title);
			if (self.test_url) 
				url = self.test_url;
			
			$("#summaryDataBody").html('<div style="height:90px;"><img src="' + self.loading + '" border="0" /> æ­£å¨å è½½è¯æ¡æè¦...</div>');
			self.show(self.getPos(E));
			self.onloading = true;
			$.get(url, function(data, status){
				if (self.a_mouseover != E || !self.is_show) {
					return
				}
				if (status != 'success') {
					return HdDialog.loadingAlertDialogModel_1(status);
				}
				
				// å°è·åçæ°æ®è¿è¡ç¼å­ï¼é¿åéå¤é¾æ¥
				self.cache_list[title] = data;
				self.setData(data);
				if (!self.is_draged) 
					self.show(self.getPos(E));
				self.onloading = false;
			});
		}
	},
	/*
	 * å°è·åçæè¦ä¿¡æ¯æ´åå°æç¤ºæ¡åï¼ç­å¾æ¾ç¤º åæ° xml ç¸å½äº XMLHttpRequest.responseXML
	 */
	setData: function(xml){
		var flag = xml.getElementsByTagName("flag")[0].firstChild.data;
		var doctitle = xml.getElementsByTagName("doctitle")[0].firstChild.data;
		var summary = xml.getElementsByTagName("summary")[0].firstChild.data;
		var firstimg = xml.getElementsByTagName("firstimg")[0].firstChild == null ? 'null' : xml.getElementsByTagName("firstimg")[0].firstChild.data;
		var html = '';
		
		if (flag == "false") {
			html = "<p style=\"padding:5px; margin:0;\"><a href=\"/createDocAuth.do?doc_title=" + encodeURI(doctitle) + "\"><font size='2' color='red' >è¯æ¡ä¸å­å¨,è¯·ç¹å»åå»º!</font></a></p>";
		}
		else {
			if (firstimg && firstimg != 'null') {
				html = '<a href="http://www.baike.com/wiki/' + encodeURI(doctitle) + '" target="_blank"><img onload="Momo.imgload(this)" src="' + firstimg + '" style="border: 1px solid rgb(204, 204, 204); margin: 5px 10px 0pt 5px; padding: 1px; float: left;"></a>';
			}
			html += '<p style="margin: 0pt; padding: 5px;">' + summary + '<a style="padding-left: 5px;" href="http://www.baike.com/wiki/' + encodeURI(doctitle) + '" target="_blank" onclick="StatVirtualTraffic(document.referrer,window.location,\'stat_hdstat_onclick_momo_click\')">æ¥çè¯¦æ&gt;&gt;</a></p>';
		}
		$("#summaryDataBody").html(html);
	},
	// å¾çå è½½
	imgload: function(img){
		var self = this;
		var pos = self.getPos(self.a_mouseover);
		self.div.css({
			top: pos.top,
			left: pos.left
		});
	},
	// æ¾ç¤º
	show: function(pos){
		var self = this;
		self.is_show = true;
		self.is_draged = false;
		self.div.css({
			top: pos.top,
			left: pos.left
		}).show();
		if (self.scroll_hide) 
			self.win.bind('scroll', function(){
				self.hide();
			});
	},
	// éè
	hide: function(){
		var self = this;
		self.is_show = false;
		self.div.hide();
		if (self.scroll_hide) 
			self.win.unbind('scroll', function(){
				self.hide();
			});
	},
	/*
	 * é¡µé¢å è½½å®æåï¼æ§è¡æ­¤æ¹æ³
	 */
	init: function(){
		if ($("#popup").size() == 0) {
			$("body").append('<div style="width:290px;overflow:hidden;z-index:10;background-color:#d1e7fc;position:absolute;padding:5px; font-size:12px; ine-height:20px;top:-1000px;left:0;" id="popup"><p style="padding:0 0 5px 0;width:290px;margin:0;float:left;" id="headp"></p><div style="float:left;padding:5px;width:280px;background-color:#fff;line-height:18px" id="summaryDataBody"></div></div>');
		}
		else {
			if ($("#headp").size() == 0 || $("#summaryDataBody").size() == 0) {
				return HdDialog.loadingAlertDialogModel_1("Momo is black outï¼");
			}
		}
		var self = this;
		
		self.div = $(self.id);
		self.dom = $(document);
		self.win = $(window);
		// ç»ææåé¾ç»å®é¼ æ è§¦æ¸äºä»¶
		$("a.innerlink").mouseover(function(){
		    //é¼ æ ç§»å¨å°åé¾ç»è®¡
		    StatVirtualTraffic(document.referrer,window.location,'stat_hdstat_onclick_momo_show');
			if (!self.is_active || self.draging) {
				return
			}
			$(this).attr("target", "_blank");
			self.hide();
			var a = this;
			self.a_mouseover = a;
			clearTimeout(self.defer_hide);
			clearTimeout(self.defer_show);
			self.defer_show = setTimeout(function(){
				self.getData(a);
			}, self.timeout);
			
		}).mouseleave(function(){
			if (!self.is_active || self.draging) {
				return
			}
			clearTimeout(self.defer_show);
			self.defer_hide = setTimeout(function(){
				self.hide();
			}, 1500);
		});
		
		
		self.div.mouseover(function(){
			clearTimeout(self.defer_hide);
		}).dblclick(function(){
			self.hide();
		}).mouseleave(function(){
			clearTimeout(self.defer_hide);
			self.defer_hide = setTimeout(function(){
				self.hide();
			}, 1500);
		}).css({
			visibility: 'visible'
		}).hide();
		
		$("#headp").html('<span style="float: left; font-size: 12px; margin-left: 5px;"><img src="http://www.huimg.cn/wikibar/images/momo/momo_3.gif" align="absmiddle"> <a target="_blank" style="color: rgb(255, 0, 0); text-decoration: none;" href="">title</a></span><span style="float: right; margin-right: 5px;cursor: pointer;"><img name="close" src="http://www.huimg.cn/wikibar/images/momo/guanbi_normal.gif" width="14" border="0" height="14"></span>');
		
		$("#headp").find("img[name=close]").click(function(){
			self.hide();
		});
		
		// ç»å®ç§»å¨äºä»¶
		$("#headp").mousedown(function(e){
			e = e || event;
			if (!self.movable) {
				return
			}
			self.draging = 1;
			var offset = self.div.offset();
			var body = $(document.body).offset();
			self.offsetClick = {
				width: e.pageX - offset.left + body.left,
				height: e.pageY - offset.top,
				startX: offset.left,
				startY: offset.top
			};
			self.div.unbind("mouseleave");
			$(document).mousemove(function(e){
				self.move(e);
			}).bind("selectstart", function(){
				return false;
			});
			
			$(document).mouseup(function(e){
				var oc = self.offsetClick;
				var pos = {
					left: oc.startX,
					top: oc.startY
				};
				self.draging = 0;
				if (e.clientY < 0 || e.clientX < 2 ||
				e.clientX > $(document).width() ||
				e.clientY > $(window).height()) {
					self.div.css(pos);
				}
				$(document).unbind("mousemove").unbind("selectstart").unbind("mouseup");
				self.div.mouseleave(function(){
					clearTimeout(self.defer_hide);
					self.defer_hide = setTimeout(function(){
						self.hide();
					}, 1500);
				});
				return false;
			});
		});
	},
	
	// ç§»å¨
	move: function(e){
		e = e || event;
		var pageX, pageY, self = this;
		pageX = e.pageX || (e.clientX + $(document).scrollLeft());
		pageY = e.pageY || (e.clientY + $(document).scrollTop());
		
		var x = self.offsetClick;
		self.div.css({
			left: pageX - x.width,
			top: pageY - x.height
		});
		
		self.is_draged = true;
	},
	
	// æå¼ momo åè½
	setOn: function(){
		this.is_active = 1;
	},
	
	// å³é­ momo åè½
	setOff: function(){
		this.is_active = 0;
		this.hide();
	}
};
/*
 * æä¾å vçç¼è¾èçæµ®å¨å¼¹åºæ¡ MomoForVEditor @required jQuery @ site http://www.baike.com @
 * version 1.0 @ date 2011.11.11 10:29 @ author: zhaoc copy from Momo object
 * upside the js file
 */
var MomoForVEditor = {
	// æ¯å¦å¯ä»¥ä½¿ç¨
	is_active: 1,
	
	// ææè·åè¿æ°æ®çç¼å­ä¿¡æ¯ï¼é¿åéå¤é¾æ¥
	cache_list: [],
	
	// æµè¯æ°æ®çurlï¼å¦ææ¯æ­£å¼ä½¿ç¨è¯·ç½®ç©º
	test_url: '',
	
	// è·åæ°æ®çURL
	url: '/wikdoc/sp/validateDocSummary.do',
	
	// é¼ æ æ¸å°ç <a> æ ç­¾å¯¹è±¡
	a_mouseover: null,
	
	// æ¾ç¤ºå»¶è¿æ è¯ï¼ç¨äºè®°å½setTimeoutçè¿åå¼
	defer_show: 0,
	
	// éèå»¶è¿æ è¯ï¼ç¨äºè®°å½setTimeoutçè¿åå¼
	defer_hide: 0,
	
	// è§¦æ¸å»¶è¿ï¼å»¶è¿æ­¤æ¶é´åè§¦åæ¾ç¤ºäºä»¶
	timeout: 500,
	
	/*
	 * æ¾ç¤ºæ¡divççidï¼å½åçç¨åºä¾èµæ¨¡æ¿çæ¾ç¤ºæ¡çè®¾è®¡ï¼å¦ä¸ï¼
	 * 
	 * <div id="renzheng"> <em class="rzjiao-t"></em> <em class="rztiao-t"></em>
	 * <div class="rz-contant zoom"> <div class="fefefd"> <h3><span
	 * class="zyrzico"></span>ä¸ä¸è®¤è¯</h3> <a href="#" target="_blank" class="r
	 * more">ä»ä¹æ¯ä¸ä¸è®¤è¯&gt;&gt;</a> <p>è¯ºè´å°ç»æµå­¦å¥å¨ç§°ä¸º"çºªå¿µé¿å°å¼é·å¾·Â·è¯ºè´å°çå¸é¶è¡ç»æµå­¦ã</p> </div>
	 * </div> <em class="rztiao-b"></em> <em class="rzjiao-b"></em> </div>
	 */
	id: 'renzheng',
	
	// æ¯å¦å¯ä»¥é¼ æ æå¨
	movable: true,
	
	// æ¯å¦å¤äºæå¨å½ä¸­
	draging: false,
	
	// æ¯å¦è¢«æå¨äº
	is_draged: false,
	
	// æ¯å¦å¤äºæ¾ç¤ºç¶æ
	is_show: false,
	
	// æ¾ç¤ºå è½½è¯æ¡æè¦è¿ç¨çå¨æå¾ç
	loading: 'http://www.huimg.cn/public/loading.gif',
	
	// æ­£å¨å è½½
	onloading: false,
	
	win: null,
	dom: null,
	div: null,
	
	// çªå£æ»å¨æ¶éè
	scroll_hide: false,
	
	// è·åæç¤ºæ¡çæä½³æ¾ç¤ºåæ 
	getPos: function(E){
		var self = this;
		var left, top, a = $(E), div = self.div, dom = self.dom, win = self.win;
		var dW = div.width(), dH = div.height();
		var pos = a.offset(), aH = a.height(), aW = a.width();
		var cW = win.width(), cH = win.height(), sL = dom.scrollLeft(), sT = dom.scrollTop();
		var body_left = (document.documentElement.clientWidth - document.body.clientWidth) / 2;
		
		// ä¸é¢ç 0.8 ç­æ¯ä¸ºäºè°æ´å½åé¾ä½ç½®åé¡µé¢çªå£å³ä¾§æ¶ï¼å¨åé¾çå·¦ä¾§æ¹åæ¾ç¤ºæç¤ºæ¡
		var isW = pos.left + dW < cW * 0.8 + sL;
		
		// ä¸é¢ç 0.9 ç­æ¯ä¸ºäºè°æ´å½åé¾ä½ç½®åé¡µé¢çªå£ä¸ä¾§æ¶ï¼å¨åé¾çä¸ä¾§æ¹åæ¾ç¤ºæç¤ºæ¡
		// åæ§ä»¶æ¯0.9ï¼ç°æ¹ä¸º0.4ï¼è®©å¼¹åºæ¡å°½éå¨ä¸æ¹æ¾ç¤º
		var isH = pos.top + dH < cH * 0.9 + sT;
		
// left = isW ? pos.left : pos.left - dW + aW;
		
		left = pos.left - 42;
		// top = isH ? pos.top + aH + 2 : pos.top - dH - aH + 25; // +25å¾®è°ä¸ä¸
		top = isH ? pos.top + aH + 2 + 10 : pos.top - dH - 25 + 18;		// +18å¾®è°ä¸ä¸
																	// ahæ´æ¹ä¸ºåºå®å¼TODO:è¿ä¸ªæ¡æ¶çè¿ä¸ªå®ä½ç®æ³ä¸å¤ªåéï¼è¯¥æ¹è¿
		
		// å¤æ­æåç®­å¤´çæå
		var arrowDirection = isH ? "up" : 'down';
		
		if(body_left > 0) {
			left = left - body_left;
		}
		return {
			left: left - 20,		// å¾®è°
			top: top,
			arrowDirection: arrowDirection
		};
	},
	// è·åè¯æ¡çæè¦ä¿¡æ¯
	getData: function(E){
		var self = this;
		var userIden = $(E).attr('uiden');
		
		if(typeof(userIden) == 'undefined' || userIden == ""){
			return;
		}
		
		$('.rz-contant', self.div).html('<img src="' + self.loading + '" border="0" /> å è½½ä¸­...');
		if (self.cache_list[userIden]) {
			self.setData(self.cache_list[userIden]);
			self.show(self.getPos(E));
		}
		else {
			self.show(self.getPos(E));
			self.onloading = true;
			var url = "http://i.baike.com/userCardInfo.do?userIden=" + userIden + "&callback=?";
			$.getJSON(url, function(json){
				if (self.a_mouseover != E || !self.is_show) {
					return;
				}
				// å¦æè¿åæ°æ®å¼å¸¸
				if(json == null || json.flag == null) {
					self.setData({flag:'0', resultInfo:'æ°æ®å¼å¸¸ï¼è¯·ç¨ååè¯'});
					if (!self.is_draged){
						self.show(self.getPos(E));
					}
					self.onloading = false;
					return;
				}
				
				var flag = json.flag;
				if (flag == 1){
					// å°è·åçæ°æ®è¿è¡ç¼å­ï¼é¿åéå¤é¾æ¥
					self.cache_list[userIden] = json;
					self.setData(json);
					if (!self.is_draged)
						self.show(self.getPos(E));
					self.onloading = false;
				}else{
					self.setData(json);
					if (!self.is_draged)
						self.show(self.getPos(E));
					self.onloading = false;
				}
			});
		}
	},
	/*
	 * å°è·åçæè¦ä¿¡æ¯æ´åå°æç¤ºæ¡åï¼ç­å¾æ¾ç¤º åæ° resultInfo ç¸å½äº è¿åå¼ä¸­json
	 */
	setData: function(json){
		var self = this;
		if(json.flag == '1') {
			var firstFirst;
			if(json.first!=null){
				firstFirst = json.first[0];
			}
			var subject = "";
			var expert="";
			if(json.other!=null){
				var other = json.other;
				for(var i=0;i < other.length; i++){
					if(other[i].displayType == "1"){
						expert += '<span class="' + other[i].showIco + '" title="' + other[i].titleName +'" ></span>';
					}
					if(other[i].displayType == "2"){
						subject += '<a title="">' + other[i].titleName + '</a>';
					}
				}
			}
			
			var html ='';
			html +='<div class="u-fl">';
			html += '<a href="http://i.baike.com/profile.do?useriden='+ json.userIden +'&prd=doc_visitingcard_userpic" class="aimg" target="_blank" alt="" ><img src="'+json.userImg+'" width="50px"/></a>';
			html += expert;
			html += '</div>';
			//å³æ³¨
			if(json.isFriend == 0){
				html += '<div class="r" id="follow_detail"><span class="red" id="follow_prompt"></span><a href="javascript:void(0)" class="btn-togz" onclick="followRelation(1,\'' + json.userIden + '\',this);StatVirtualTraffic(document.referrer,window.location,\'stat_hdstat_onclick_usercard_follow\');return false;">å³æ³¨</a></div>';
			}else if(json.isFriend == 1){
				html += '<div class="r" id="follow_detail"><span class="red" id="follow_prompt"></span><span class="btn-yigz">å·²å³æ³¨<a  onclick="followRelation(0,\'' + json.userIden + '\',this);StatVirtualTraffic(document.referrer,window.location,\'stat_hdstat_onclick_usercard_cancelfollow\');return false;" href="javascript:void(0)" >åæ¶</a></span></div>';
			}else if(json.isFriend == 2){
				html += '<div class="r" id="follow_detail"><span class="red" id="follow_prompt"></span><span class="btn-yigz">å·²å³æ³¨<a  onclick="followRelation(0,\'' + json.userIden + '\',this);StatVirtualTraffic(document.referrer,window.location,\'stat_hdstat_onclick_usercard_cancelfollow\');return false;" href="javascript:void(0)" >åæ¶</a></span></div>';
			}else if(json.isFriend == 3){
			}
			//åå­+è®¤è¯ä¿¡æ¯
			html += '<h3><a href="http://i.baike.com/profile.do?useriden='+ json.userIden +'&prd=doc_visitingcard_username" target="_blank">' + subStrTitle(json.userNick,7) + '</a>';
			//æè®¤è¯ä¿¡æ¯æ¾ç¤ºè®¤è¯ä¿¡æ¯ï¼å¦åæ¾ç¤ºç¨æ·ç®ä»
			if(firstFirst != null&&firstFirst.displayType == "1"){
				html += '<span class="zyrzico"></span></h3>';
				if(firstFirst.authenticationInfo!=null){
					html += '<p>' + subStr(firstFirst.authenticationInfo,30) + '</p>';
				}
			}else{
				html += '</h3>';
				if(json.userIntro!=null){
					html += '<p>' + subStr(json.userIntro,30) + "</p>";
				}
			}
			//å¶ä»å¤´è¡
			html += '<p class="tag">' + subject + '</p>';
			html += '<p class="ver">åä½çæ¬æ°ï¼<i>' + json.userCollaboratedDocCount + '</i></p>';
			html += '<ul id="properson-content">';
			html += '<li><a href="http://i.baike.com/followList.do?useriden=' + json.userIden + '&prd=doc_visitingcard_follow" target="_blank">å³æ³¨</a><a id="followCount" class="digit" target="_blank" href="http://i.baike.com/followList.do?useriden=' + json.userIden + '">' + json.followCount + '</a></li>';
			html += '<li><a href="http://i.baike.com/fansList.do?useriden=' + json.userIden + '&prd=doc_visitingcard_fans" target="_blank">ç²ä¸</a><a id="fansCount" class="digit" target="_blank" href="http://i.baike.com/fansList.do?useriden=' + json.userIden + '">' + json.fansCount + '</a></li>';
			html += '<li><a href="http://i.baike.com/profile.do?useriden=' + json.userIden + '&prd=doc_visitingcard_news" target="_blank">æ°é²äº</a><a id="userfeedCount" class="digit" target="_blank" href="http://i.baike.com/profile.do?useriden=' + json.userIden + '">' + json.userfeedCount + '</a></li>';
			html += '</ul>';
			
			$('.rz-contant', self.div).empty().append($(html));
		}else {
			$('.rz-contant', self.div).html("æ±æ­ï¼ç¨æ·åçè·åå¤±è´¥ï¼");
		}
	},
	// å¾çå è½½
	imgload: function(img){
		var self = this;
		var pos = self.getPos(self.a_mouseover);
		self.div.css({
			top: pos.top,
			left: pos.left
		});
	},
	// æ¾ç¤º
	show: function(pos){
		var self = this;
		self.is_show = true;
		self.is_draged = false;
		
		// å¤æ­ç®­å¤´
		if(pos.arrowDirection == 'up') {
			$('#rzjiao-t', self.div).show();
			$('#rzjiao-b', self.div).hide();
		}else if(pos.arrowDirection == "down") {
			$('#rzjiao-t', self.div).hide();
			$('#rzjiao-b', self.div).show();
		}
		
		self.div.css({
			top: pos.top,
			left: pos.left
		}).show();
		if (self.scroll_hide) {
			self.win.bind('scroll', function(){
				self.hide();
			});
		}
	},
	// éè
	hide: function(){
		var self = this;
		self.is_show = false;
		self.div.hide();
		if (self.scroll_hide) 
			self.win.unbind('scroll', function(){
				self.hide();
			});
	},
	/**
	 * é¡µé¢å è½½å®æåï¼æ§è¡æ­¤æ¹æ³
	 */
	init: function(){
		var self = this;
		
		if ($("#" + self.id).length == 0) {
			$("body").append('<div id="'+self.id+'" style="position:absolute;z-index:100"><em class="bg"></em><em class="rztiao-t" id="rzjiao-b" style="display:none;"></em><div class="rz-contant"></div><em class="rzjiao-t" id="rzjiao-t" style="display:none;"></em></div>');
		}
		
		self.div = $("#" + self.id);
		self.dom = $(document);
		self.win = $(window);
		
		// ç»ææåé¾ç»å®é¼ æ è§¦æ¸äºä»¶
		$(".vEditor").mouseover(function(){
			if (!self.is_active || self.draging) {
				return
			}
			self.hide();
			var a = this;
			self.a_mouseover = a;
			clearTimeout(self.defer_hide);
			clearTimeout(self.defer_show);
			self.defer_show = setTimeout(function(){
				self.getData(a);
			}, self.timeout);
			
		}).mouseleave(function(){
			if (!self.is_active || self.draging) {
				return
			}
			clearTimeout(self.defer_show);
			self.defer_hide = setTimeout(function(){
				self.hide();
			}, 500);
		});
		
		
		self.div.mouseover(function(){
			clearTimeout(self.defer_hide);
		}).mouseleave(function(){
			clearTimeout(self.defer_hide);
			self.defer_hide = setTimeout(function(){
				self.hide();
			}, 500);
		}).css({
			visibility: 'visible'
		}).hide();
	},
	
	// ç§»å¨
	move: function(e){
		e = e || event;
		var pageX, pageY, self = this;
		pageX = e.pageX || (e.clientX + $(document).scrollLeft());
		pageY = e.pageY || (e.clientY + $(document).scrollTop());
		
		var x = self.offsetClick;
		self.div.css({
			left: pageX - x.width,
			top: pageY - x.height
		});
		
		self.is_draged = true;
	},
	
	// æå¼ momo åè½
	setOn: function(){
		this.is_active = 1;
	},
	
	// å³é­ momo åè½
	setOff: function(){
		this.is_active = 0;
		this.hide();
	}
};

function HdLoadJs(jsUrl, fun){
	$.ajax({
		type: "GET",
		dataType:'script',
		url:jsUrl,
		cache:true,
		success:fun
	});
}

