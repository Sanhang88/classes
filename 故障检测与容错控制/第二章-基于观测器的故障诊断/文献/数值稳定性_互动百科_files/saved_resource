$(window).ready(function(){
	var zutuImgList =$("#fodder").find("ul img");
	var summaryimgObj = $("div[class=summary]").find("img");
	if (jQuery.browser.msie && jQuery.browser.version <= 6) {
		if(zutuImgList.length>0){
			zutuImgList.each(function(l) {
				resizeImage(this, 120, 120);
			});
		}
		if(summaryimgObj.length>0){
			summaryimgObj.each(function(l) {
				resizeImage(this, 120, 120);
			});
		}
	}
});

