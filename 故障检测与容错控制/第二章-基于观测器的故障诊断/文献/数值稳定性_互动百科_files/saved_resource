function newshowCorrectionDiv(type){
	if (loadallcerrectionjsflag) {
		showCorrectionDiv(type);
	}
	else {
		jQuery.getScript("/js/allcerrection20120328.js", function(){
			showCorrectionDiv(type);
			loadallcerrectionjsflag = true;
		});
	}
}
