var xmlHttp;
var IE = document.all ? true : false;
var hdCookieName = "hduser";
function personalCheck(){
	
	var doc_title = $("#doctitle").val();
	
		var url = "http://guancha.baike.com/personalBaikeApi.do?action=perfession&docTitle="+doc_title+"&callback=?";
		$.getJSON(url, function(json){
			var cateNum = json.num;
			
			if(cateNum>0){
				
				var map = eval(json.map);	
				var doc_title_sub = doc_title.substr(0,doc_title.indexOf('['));
				
				var htmlText = '<h3>è¿ä½ä¸æ¯ä½ è¦æ¾ç'+doc_title_sub+'ï¼äººç©ç¾ç§ä¸­è¿æ'+cateNum+'ä½<strong>å¶ä»è¡ä¸ç'+doc_title_sub+'</strong>ï¼éæ©ä½ è¦ççèä¸:</h3><ul>';
				
				for(var key in map){
					htmlText+='<li><a href="http://so.baike.com/doc/'+doc_title_sub+'" target="_blank">'+key+'['+map[key]+']</a></li>';
				}
				htmlText+='</ul><div class="clear"></div>';
				
				$("#personal_polysemy").html(htmlText);
				$("#personal_polysemy").show();
			}
		});	
}

function checkIE6() {
	var nav = navigator.userAgent;
	if (/MSIE [78]/.test(nav))
		return false;
	if (/MSIE [56]/.test(nav))
		return true;
	return false;
}
var isIE6 = checkIE6();
function createXMLHttpRequest() {
	if (window.ActiveXObject) {
		xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");
	} else if (window.XMLHttpRequest) {
		xmlHttp = new XMLHttpRequest();
	}
	if (!xmlHttp) { // å¼å¸¸ï¼åå»ºå¯¹è±¡å®ä¾å¤±è´¥
		HdDialog.loadingAlertDialogModel_1("ä¸è½åå»ºXMLHttpRequestå¯¹è±¡å®ä¾ï¼");
		return false;
	}
}
function createXMLHttpRequestByObject(xmlObject) {
	if (window.ActiveXObject) {
		xmlObject = new ActiveXObject("Microsoft.XMLHTTP");
	} else if (window.XMLHttpRequest) {
		xmlObject = new XMLHttpRequest();
	}
	if (!xmlObject) { // å¼å¸¸ï¼åå»ºå¯¹è±¡å®ä¾å¤±è´¥
		HdDialog.loadingAlertDialogModel_1("ä¸è½åå»ºXMLHttpRequestå¯¹è±¡å®ä¾ï¼");
		return false;
	}
	return xmlObject;
}
// ç¨äºæªåå­ç¬¦ä¸²
String.prototype.Trim = function() {
	return this.replace(/^\s*|\s*$/g, "");
};
// å»å¤ä½çç©ºæ ¼
function KillSpace(text) {
	var i = 0, len = 0, lentmp = 0, a = 0, b = 0;
	while (i == 0) {
		lentmp = text.length;
		text = text.replace(/(^\s*)|(\s*$)/g, "");
		len = text.length;
		if (lentmp == len) {
			a = 0;
		} else {
			a = 1;
		}
		lentmp = text.length;
		text = text.replace(/(^\u3000*)|(\u3000*$)/g, "");
		text = text.replace(/(^\ue4c6*)|(\ue4c6*$)/g, "");
		len = text.length;
		if (lentmp == len) {
			b = 0;
		} else {
			b = 1;
		}
		if (a == 0 && b == 0) {
			i = 1;
		} else {
			i = 0;
		}
	}
	return text;
}
// éªè¯ç 
function showsec(id, type) {
	var objseccode = document.getElementById("seccode");
	objseccode.value = '';
	var objImage = document.getElementById(id);
	str = "/Captcha.jpg?random=" + Math.random();
	if (type && type != '') {
		str += "&type=" + type;
	}
	objImage.src = str;
}
function showsecNew(id, type) {
	var objImage = document.getElementById(id);
	str = "/Captcha.jpg?random=" + Math.random();
	if (type && type != '') {
		str += "&type=" + type;
	}
	objImage.src = str;
}
// ånext_urlçcookie;nextURL;hoodong_wiki_domain = ".baike.com"
function setCookie(name, value) {
	var Days = 1; // æ­¤ cookie å°è¢«ä¿å­ 1 å¤©
	var exp = new Date(); // new Date("December 31, 9998");
	exp.setTime(exp.getTime() + Days * 24 * 60 * 60 * 1000);
	document.cookie = name + "=" + escape(value) + ";expire="
			+ exp.toGMTString();
}
function checkDocTitle(checkedDocTitle) {
	if (checkedDocTitle == '') {
		return 'error0';
	}
	var htmlpatrn = '(<.*\/>)|(<.*>.*<\/.*>)';
	if (checkedDocTitle.match(htmlpatrn)) {
		return 'error2';
	}
	var codepattn = "[ï¼»ï¼½ããï¼<>ãã`~!@#ï¼$%ï¼^&*ï¼\|;ï¼$+]";
	if (checkedDocTitle.match(codepattn) || checkedDocTitle.indexOf('\\') != -1) {
		return 'error1';
	}
	return 'success';
}
/**
 * æ£æ¥æ¶æ­§ä¹
 * 
 * @param {Object}
 *            checkedDocTitle
 */
function checkAmbigDocTitle(checkedDocTitle){
	if (checkedDocTitle == '') {
		return 'error0';
	}
	var htmlpatrn = '(<.*\/>)|(<.*>.*<\/.*>)';
	if (checkedDocTitle.match(htmlpatrn)) {
		return 'error2';
	}
	var codepattn = "[ï¼»ï¼½ãã\[ï¼<>\\]ãã`~!@#ï¼$%ï¼^&*ï¼\|;ï¼$+]";
	if (checkedDocTitle.match(codepattn) || checkedDocTitle.indexOf('\\') != -1) {
		return 'error1';
	}
	return 'success';
}
/**
 * è®¾ç½®iframeé«åº¦ï¼èªéåº
 */
function setIframeHeight(fm_id){
	var frm = document.getElementById(fm_id);
	if (frm != null) {
		setTimeout(function(){
			if (document.all || navigator.userAgent.indexOf('Chrome') > -1) {
				frm.style.height = frm.contentWindow.document.body == null ? "0px" : (frm.contentWindow.document.body.scrollHeight + "px");
			}
			else {
				frm.style.height = frm.contentDocument.documentElement.scrollHeight + "px";
			}
		}, 200);
	}
}
/**
 * å­ç¬¦ä¸²æ±é¿åº¦(å¨è§)
 */
function HD_getLength(str){
	var len;
	var i;
	len = 0;
	for (i = 0; i < str.length; i++) {
		if (str.charCodeAt(i) > 255) 
			len += 2;
		else 
			len++;
	}
	return len;
}
/**
 * å­ç¬¦ä¸²æªåé¨å(å¨è§)
 */
function HD_subString(str, len){
	var newLength = 0;
	var newStr = "";
	var chineseRegex = /[^\x00-\xff]/g;
	var singleChar = "";
	var strLength = str.replace(chineseRegex, "**").length;
	for (var i = 0; i < strLength; i++) {
		singleChar = str.charAt(i).toString();
		if (singleChar.match(chineseRegex) != null) {
			newLength += 2;
		}
		else {
			newLength++;
		}
		if (newLength > len) {
			break;
		}
		newStr += singleChar;
	}
	return newStr;
}
