(function($) {
	$.fn.extend({
				autocomplete : function(urlOrData, options) {
					var isUrl = typeof urlOrData == "string";
					options = $.extend({}, $.Autocompleter.defaults, {
								url : isUrl ? urlOrData : null,
								data : isUrl ? null : urlOrData,
								delay : isUrl
										? $.Autocompleter.defaults.delay
										: 10,
								max : options && !options.scroll ? 10 : 150
							}, options);
					options.highlight = options.highlight || function(value) {
						return value;
					};
					options.formatMatch = options.formatMatch
							|| options.formatItem;
					return this.each(function() {
								new $.Autocompleter(this, options);
							});
				},
				result : function(handler) {
					return this.bind("result", handler);
				},
				search : function(handler) {
					return this.trigger("search", [handler]);
				},
				flushCache : function() {
					return this.trigger("flushCache");
				},
				setOptions : function(options) {
					return this.trigger("setOptions", [options]);
				},
				unautocomplete : function() {
					return this.trigger("unautocomplete");
				}
			});
	$.Autocompleter = function(input, options) {
		var KEY = {
			UP : 38,
			DOWN : 40,
			DEL : 46,
			TAB : 9,
			RETURN : 13,
			ESC : 27,
			COMMA : 188,
			PAGEUP : 33,
			PAGEDOWN : 34,
			BACKSPACE : 8
		};
		var $input = $(input).attr("autocomplete", "off").addClass(options.inputClass);
		var timeout;
		var previousValue = "";
		var cache = $.Autocompleter.Cache(options);
		var hasFocus = 0;
		var lastKeyPressCode;
		var config = {
			mouseDownOnSelect : false
		};
		var select = $.Autocompleter.Select(options, input, selectCurrent,
				config);
		var blockSubmit;
		$.browser.opera
				&& $(input.form).bind("submit.autocomplete", function() {
							if (blockSubmit) {
								blockSubmit = false;
								return false;
							}
						});
		$input.bind(
				($.browser.opera ? "keypress" : ($.browser.msie
						? "keydown"
						: "keyup"))
						+ ".autocomplete", function(event) {
					lastKeyPressCode = event.keyCode;
					switch (event.keyCode) {
						case KEY.PAGEUP :
							event.preventDefault();
							if (select.visible()) {
								select.pageUp();
							} else {
								onChange(0, true);
							}
							break;
						case KEY.PAGEDOWN :
							event.preventDefault();
							if (select.visible()) {
								select.pageDown();
							} else {
								onChange(0, true);
							}
							break;
						case options.multiple
								&& $.trim(options.multipleSeparator) == ","
								&& KEY.COMMA :
						case KEY.TAB :
						case KEY.RETURN :
							$(input).click();
							if (selectCurrent()) {
								event.preventDefault();
								blockSubmit = true;
								if (autokind_flag == 0) {
									var next = jQuery(input).next();
									if (next.attr("name") == undefined) {
										input.focus();
									} else {
										next.focus().click();
									}
								} else if (autokind_flag == 1) {
									var parent = jQuery(input).parent();
									var next = parent.next();
									var children = next.children(".tag_input");
									if (children.attr("name") == undefined) {
										input.focus();
									} else {
										children.focus();
									}
								} else if (autokind_flag == 2) {
									var parent = jQuery(input).parent();
									var next = parent.next();
									var children = next.children(".inp-btn");
									if (children.attr("name") == undefined) {
										input.focus();
									} else {
										children.focus().click();
									}
								}
								return false;
							}
							if (jQuery.browser.mozilla == true) {
								onChange(0, true);
							}
							break;
						case KEY.ESC :
							select.hide();
							break;
						default :
							clearTimeout(timeout);
							timeout = setTimeout(onChange, options.delay);
							break;
					}
				}).focus(function() {
					hasFocus++;
				}).blur(function() {
					hasFocus = 0;
					if (!config.mouseDownOnSelect) {
						hideResults();
					}
				}).click(function() {
					if (hasFocus++ > 1 && !select.visible()) {
						onChange(0, true);
					}
				}).bind("search", function() {
			var fn = (arguments.length > 1) ? arguments[1] : null;
			function findValueCallback(q, data) {
				var result;
				if (data && data.length) {
					for (var i = 0; i < data.length; i++) {
						if (data[i].result.toLowerCase() == q.toLowerCase()) {
							result = data[i];
							break;
						}
					}
				}
				if (typeof fn == "function")
					fn(result);
				else
					$input.trigger("result", result
									&& [result.data, result.value]);
			}
			$.each(trimWords($input.val()), function(i, value) {
						request(value, findValueCallback, findValueCallback);
					});
		}).bind("flushCache", function() {
					cache.flush();
				}).bind("setOptions", function() {
					$.extend(options, arguments[1]);
					if ("data" in arguments[1])
						cache.populate();
				}).bind("unautocomplete", function() {
					select.unbind();
					$input.unbind();
					$(input.form).unbind(".autocomplete");
				});
		$input.keydown(function(e) {
					lastKeyPressCode = e.keyCode;
					switch (e.keyCode) {
						case 13 :
							if (selectCurrent()) {
								$input.get(0).blur();
								e.preventDefault();
								if (autokind_flag == 0) {
									var next = jQuery(input).next();
									if (next.attr("name") == undefined) {
										input.focus();
									} else {
										next.focus().click();
									}
								} else if (autokind_flag == 1) {
									var parent = jQuery(input).parent();
									var next = parent.next();
									var children = next.children(".tag_input");
									if (children.attr("name") == undefined) {
										input.focus();
									} else {
										children.focus();
									}
								} else if (autokind_flag == 2) {
									var parent = jQuery(input).parent();
									var next = parent.next();
									var children = next.children(".inp-btn");
									if (children.attr("name") == undefined) {
										input.focus();
									} else {
										children.focus().click();
									}
								}
								if(isEnterJump&&input.name=='q'){
									if(isEnterJumpdoc){
										location = "http://www.baike.com/wiki/"+encodeURIComponent(input.value)+"&prd=so_auto_doc_list";
									}else{
										location = "http://so.baike.com/doc/"+encodeURIComponent(input.value)+"&prd=so_auto_doc_list";	
									}
								}
							}
							break;
						case KEY.UP :
							e.preventDefault();
							if (select.visible()) {
								select.prev();
							} else {
								onChange(0, true);
							}
							selectCurrent2();
							break;
						case KEY.DOWN :
							e.preventDefault();
							if (select.visible()) {
								select.next();
							} else {
								onChange(0, true);
							}
							selectCurrent2();
							break;
					}
				});
		function selectCurrent() {
			var selected = select.selected();
			if (!selected)
				return false;
			var v = selected.result;
			previousValue = v;
			if (options.multiple) {
				var words = trimWords($input.val());
				if (words.length > 1) {
					v = words.slice(0, words.length - 1)
							.join(options.multipleSeparator)
							+ options.multipleSeparator + v;
				}
				v += options.multipleSeparator;
			}
			$input.val(v);
			hideResultsNow();
			$input.trigger("result", [selected.data, selected.value]);
			return true;
		}
		function selectCurrent2() {
			var selected = select.selected();
			if (!selected)
				return false;
			var v = selected.result;
			previousValue = v;
			if (options.multiple) {
				var words = trimWords($input.val());
				if (words.length > 1) {
					v = words.slice(0, words.length - 1)
							.join(options.multipleSeparator)
							+ options.multipleSeparator + v;
				}
				v += options.multipleSeparator;
			}
			$input.val(v);
			$input.trigger("result", [selected.data, selected.value]);
			return true;
		}
		function onChange(crap, skipPrevCheck) {
			if (lastKeyPressCode == KEY.DEL) {
				select.hide();
				return;
			}
			var currentValue = $input.val();
			if (!skipPrevCheck && currentValue == previousValue)
				return;
			previousValue = currentValue;
			currentValue = lastWord(currentValue);
			if (currentValue.length >= options.minChars) {
				$input.addClass(options.loadingClass);
				if (!options.matchCase)
					currentValue = currentValue.toLowerCase();
				request(currentValue, receiveData, hideResultsNow);
			} else {
				stopLoading();
				select.hide();
			}
		};
		function trimWords(value) {
			if (!value) {
				return [""];
			}
			var words = value.split(options.multipleSeparator);
			var result = [];
			$.each(words, function(i, value) {
						if ($.trim(value))
							result[i] = $.trim(value);
					});
			return result;
		}
		function lastWord(value) {
			if (!options.multiple)
				return value;
			var words = trimWords(value);
			return words[words.length - 1];
		}
		function autoFill(q, sValue) {
			if (options.autoFill
					&& (lastWord($input.val()).toLowerCase() == q.toLowerCase())
					&& lastKeyPressCode != KEY.BACKSPACE) {
				$input.val($input.val()
						+ sValue.substring(lastWord(previousValue).length));
				$.Autocompleter.Selection(input, previousValue.length,
						previousValue.length + sValue.length);
			}
		};
		function hideResults() {
			clearTimeout(timeout);
			timeout = setTimeout(hideResultsNow, 200);
		};
		function hideResultsNow() {
			var wasVisible = select.visible();
			select.hide();
			clearTimeout(timeout);
			stopLoading();
			if (options.mustMatch) {
				$input.search(function(result) {
							if (!result) {
								if (options.multiple) {
									var words = trimWords($input.val()).slice(
											0, -1);
									$input.val(words
											.join(options.multipleSeparator)
											+ (words.length
													? options.multipleSeparator
													: ""));
								} else
									$input.val("");
							}
						});
			}
			if (wasVisible)
				$.Autocompleter.Selection(input, input.value.length,
						input.value.length);
		};
		function receiveData(q, data) {
			if (data && data.length && (data[0].value == "cannotfindmatchword")) {
				data.pop();
			} 
			if (data && data.length && hasFocus
					&& (data[0].value != "cannotfindmatchword")) {
				stopLoading();
				select.display(data, q);
				autoFill(q, data[0].value);
				select.show();
			} else {
				hideResultsNow();
			}
			//add bing
			if(options.showBing){
				select.display(data, q);
				auto_bing.request(q,data);
				select.show();
			}
		};
		function request(term, success, failure) {
			if (!options.matchCase)
				term = term.toLowerCase();
			var data = cache.load(term);
			if (data && data.length && ifcache) {
				success(term, data);
			} else if ((typeof options.url == "string")
					&& (options.url.length > 0)) {
				var extraParams = {
					timestamp : +new Date()
				};
				$.each(options.extraParams, function(key, param) {
							extraParams[key] = typeof param == "function"
									? param()
									: param;
						});
				$.ajax({
							mode : "abort",
							port : "autocomplete" + input.name,
							dataType : options.dataType,
							url : options.url,
							data : $.extend({
										q : lastWord(term),
										limit : options.max
									}, extraParams),
							success : function(data) {
								var parsed = options.parse
										&& options.parse(data) || parse(data);
								cache.add(term, parsed);
								success(term, parsed);
							}
						});
			} else {
				select.emptyList();
				failure(term);
			}
		};
		function parse(data) {
			var parsed = [];
			var rows = data.split("\n");
			for (var i = 0; i < rows.length; i++) {
				var row = $.trim(rows[i]);
				if (row) {
					row = row.split("|");
					parsed[parsed.length] = {
						data : row,
						value : row[0],
						result : options.formatResult
								&& options.formatResult(row, row[0]) || row[0]
					};
				}
			}
			return parsed;
		};
		function stopLoading() {
			$input.removeClass(options.loadingClass);
		};
	};
	$.Autocompleter.defaults = {
		inputClass : "ac_input",
		resultsClass : "ac_results",
		loadingClass : "ac_loading",
		minChars : 1,
		delay : 400,
		matchCase : false,
		matchSubset : true,
		matchContains : false,
		cacheLength : 10,
		max : 100,
		mustMatch : false,
		extraParams : {},
		selectFirst : true,
		formatItem : function(row) {
			return row[0];
		},
		formatMatch : null,
		autoFill : false,
		width : 0,
		multiple : false,
		multipleSeparator : ", ",
		highlight : function(value, term) {
			return value
					.replace(
							new RegExp(
									"(?![^&;]+;)(?!<[^<>]*)("
											+ term
													.replace(
															/([\^\$\(\)\[\]\{\}\*\.\+\?\|\\])/gi,
															"\\$1")
											+ ")(?![^<>]*>)(?![^&;]+;)", "gi"),
							"<strong>$1</strong>");
		},
		scroll : true,
		scrollHeight : 180
	};
	$.Autocompleter.Cache = function(options) {
		var data = {};
		var length = 0;
		function matchSubset(s, sub) {
			if (!options.matchCase)
				s = s.toLowerCase();
			var i = s.indexOf(sub);
			if (i == -1)
				return false;
			return i == 0 || options.matchContains;
		};
		function add(q, value) {
			if (length > options.cacheLength) {
				flush();
			}
			if (!data[q]) {
				length++;
			}
			data[q] = value;
		}
		function populate() {
			if (!options.data)
				return false;
			var stMatchSets = {}, nullData = 0;
			if (!options.url)
				options.cacheLength = 1;
			stMatchSets[""] = [];
			for (var i = 0, ol = options.data.length; i < ol; i++) {
				var rawValue = options.data[i];
				rawValue = (typeof rawValue == "string")
						? [rawValue]
						: rawValue;
				var value = options.formatMatch(rawValue, i + 1,
						options.data.length);
				if (value === false)
					continue;
				var firstChar = value.charAt(0).toLowerCase();
				if (!stMatchSets[firstChar])
					stMatchSets[firstChar] = [];
				var row = {
					value : value,
					data : rawValue,
					result : options.formatResult
							&& options.formatResult(rawValue) || value
				};
				stMatchSets[firstChar].push(row);
				if (nullData++ < options.max) {
					stMatchSets[""].push(row);
				}
			};
			$.each(stMatchSets, function(i, value) {
						options.cacheLength++;
						add(i, value);
					});
		}
		setTimeout(populate, 25);
		function flush() {
			data = {};
			length = 0;
		}
		return {
			flush : flush,
			add : add,
			populate : populate,
			load : function(q) {
				if (!options.cacheLength || !length)
					return null;
				if (!options.url && options.matchContains) {
					var csub = [];
					for (var k in data) {
						if (k.length > 0) {
							var c = data[k];
							$.each(c, function(i, x) {
										if (matchSubset(x.value, q)) {
											csub.push(x);
										}
									});
						}
					}
					return csub;
				} else if (data[q]) {
					return data[q];
				} else if (options.matchSubset) {
					for (var i = q.length - 1; i >= options.minChars; i--) {
						var c = data[q.substr(0, i)];
						if (c) {
							var csub = [];
							$.each(c, function(i, x) {
										if (matchSubset(x.value, q)) {
											csub[csub.length] = x;
										}
									});
							return csub;
						}
					}
				}
				return null;
			}
		};
	};
	$.Autocompleter.Select = function(options, input, select, config) {
		var CLASSES = {
			ACTIVE : "ac_over"
		};
		var listItems, active = -1, data, term = "", needsInit = true, element, list,list_bing;
		function init() {
			if (!needsInit)
				return;
			element = $("<div/>").hide().addClass(options.resultsClass).css(
					"position", "absolute").appendTo(document.body);
			list = $("<ul/>").appendTo(element);
			//bing
			if(options.showBing){
				list_bing = $("<ul/>").appendTo(element).addClass("bing");
			}
			element.mouseover(function(event) {
				if (target(event).nodeName
						&& target(event).nodeName.toUpperCase() == 'LI' && target(event).className != "hezuo") {
					active = $("li", element).removeClass(CLASSES.ACTIVE)
							.index(target(event));
					$(target(event)).addClass(CLASSES.ACTIVE);
					var selected = listItems
							&& listItems.filter("." + CLASSES.ACTIVE);
					selected = selected && selected.length
							&& $.data(selected[0], "ac_data");
					if (!selected)
						return false;
					var v = selected.result;
				}
			}).click(function(event) {
						$(target(event)).addClass(CLASSES.ACTIVE);
						select();
						if (autokind_flag == 0) {
						} else if (autokind_flag == 1) {
							var parent = jQuery(input).parent();
							var nextp = parent.next();
							var children = nextp.children(".tag_input");
							var next = children;
						} else if (autokind_flag == 2) {
							
						}
						var next = $(input).next();
						if(typeof(next)!="undefined" && next){
							if (next.attr("name") == undefined) {
								var start = input.value.length;
								var end = input.value.length;
								if (input.createTextRange) {
									var selRange = input.createTextRange();
									selRange.collapse(true);
									selRange.moveStart("character", start);
									selRange.moveEnd("character", end);
									selRange.select();
								} else if (input.setSelectionRange) {
									input.setSelectionRange(start, end);
								} else {
									if (input.selectionStart) {
										input.selectionStart = start;
										input.selectionEnd = end;
									}
								}
								input.focus();
							} else {
								next.focus();
							}
						}
//						return false;
					if(input.name=='q'){
						if(isEnterJumpdoc){
							location = "http://www.baike.com/wiki/"+encodeURIComponent(input.value)+"&prd=so_auto_doc_list";
						}else{
							location = "http://so.baike.com/doc/"+encodeURIComponent(input.value)+"&prd=so_auto_doc_list";
						}
					}
					}).mousedown(function() {
						config.mouseDownOnSelect = true;
					}).mouseup(function() {
						config.mouseDownOnSelect = false;
					});
			if (options.width > 0)
				element.css("width", options.width);
			needsInit = false;
		}
		function target(event) {
			var element = event.target;
			while (element && element.tagName != "LI")
				element = element.parentNode;
			if (!element)
				return [];
			return element;
		}
		function moveSelect(step) {
			listItems = element.find("li[class!=hezuo]");
			listItems.slice(active, active + 1).removeClass(CLASSES.ACTIVE);
			movePosition(step);
			var activeItem = listItems.slice(active, active + 1)
					.addClass(CLASSES.ACTIVE);
			if (options.scroll) {
				var offset = 0;
				listItems.slice(0, active).each(function() {
							offset += this.offsetHeight;
						});
				if ((offset + activeItem[0].offsetHeight - list.scrollTop()) > list[0].clientHeight) {
					list.scrollTop(offset + activeItem[0].offsetHeight
							- list.innerHeight());
				} else if (offset < list.scrollTop()) {
					list.scrollTop(offset);
				}
			}
		};
		function movePosition(step) {
			active += step;
			if (active < 0) {
				active = listItems.size() - 1;
			} else if (active >= listItems.size()) {
				active = 0;
			}
		}
		function limitNumberOfItems(available) {
			return options.max && options.max < available
					? options.max
					: available;
		}
		function fillList() {
			list.empty();
			if(options.showBing){
				list_bing.empty();
				list_bing.hide();
			}
			var max = limitNumberOfItems(data.length);
			for (var i = 0; i < max; i++) {
				if (!data[i])
					continue;
				var formatted = options.formatItem(data[i].data, i + 1, max,
						data[i].value, term);
				if (formatted === false)
					continue;
				var li = $("<li/>").html(options.highlight(formatted, term))
						.addClass(i % 2 == 0 ? "ac_even" : "ac_odd")
						.appendTo(list)[0];
				$.data(li, "ac_data", data[i]);
			}
			listItems = list.find("li");
			if (options.selectFirst) {
				listItems.slice(0, 1).addClass(CLASSES.ACTIVE);
				active = 0;
			}
			if ($.fn.bgiframe)
				list.bgiframe();
		}
		return {
			display : function(d, q) {
				init();
				data = d;
				term = q;
				fillList();
			},
			next : function() {
				moveSelect(1);
			},
			prev : function() {
				moveSelect(-1);
			},
			pageUp : function() {
				if (active != 0 && active - 8 < 0) {
					moveSelect(-active);
				} else {
					moveSelect(-8);
				}
			},
			pageDown : function() {
				if (active != listItems.size() - 1
						&& active + 8 > listItems.size()) {
					moveSelect(listItems.size() - 1 - active);
				} else {
					moveSelect(8);
				}
			},
			hide : function() {
				element && element.hide();
				listItems && listItems.removeClass(CLASSES.ACTIVE);
				active = -1;
			},
			visible : function() {
				return element && element.is(":visible");
			},
			current : function() {
				return this.visible()
						&& (listItems.filter("." + CLASSES.ACTIVE)[0] || options.selectFirst
								&& listItems[0]);
			},
			show : function() {
				var offset = $(input).offset();
				var newleft_ff2 = 0, newdocoff = 0, sooff = 0, ver;
				ver = jQuery.browser.version;
				if (1 == isnewdoc) {
					var bodyoff = $(document.body).offset().left;
					{
						newleft_ff2 = offset.left;
						if (jQuery.browser.msie == true) {
							if (navigator.appVersion.indexOf("MSIE 7.0") > -1) {
								newdocoff = -bodyoff;
							} else if (jQuery.browser.version == 6.0) {
								newdocoff = -bodyoff;
							}
						}
						if (jQuery.browser.mozilla == true) {
							ver = ver.substring(2, 3);
							if (ver == 8) {
							} else if (ver == 9) {
								newdocoff = -bodyoff;
							}
						}
						if (jQuery.browser.safari == true) {
							if (navigator.appVersion.indexOf("Chrome") != -1
									&& ver >= 530.5) {
								newdocoff = -bodyoff;
							}
						}
						if (jQuery.browser.opera == true) {
							newdocoff = -bodyoff;
						}
					}
				} else {
					var cW = (document.documentElement.clientWidth || document.body.clientWidth);
					var bW = $("body").width();
					if (cW - bW > 0) {
						if (isiframe == 1) {
							newleft_ff2 = offset.left;
						} else {
							newleft_ff2 = offset.left - (cW - bW) / 2 + 1;
						}
					} else {
						newleft_ff2 = offset.left;
					}
//					if (jQuery.browser.safari == true) {
//						if (navigator.appVersion.indexOf("Chrome") != -1
//								&& ver >= 530.5) {
//						} else {
//							newleft_ff2 = offset.left;
//						}
//					}
					if (jQuery.browser.opera == true) {
						if (cW - bW > 0) {
							if (isiframe == 1) {
								newleft_ff2 = offset.left;
							} else {
								newleft_ff2 = offset.left - (cW - bW) / 2;
							}
						} else {
							newleft_ff2 = offset.left;
						}
					}
					if (jQuery.browser.mozilla == true) {
						ver = ver.substring(2, 3);
						if (ver == 8) {
							newleft_ff2 = offset.left;
						} else if (ver == 9) {
							if (cW - bW > 0) {
								if (isiframe == 1) {
									newleft_ff2 = offset.left;
								} else {
									newleft_ff2 = offset.left - (cW - bW) / 2;
								}
							} else {
								newleft_ff2 = offset.left;
							}
						}
					}
					sooff = 0;
					if (isso == 1) {
						if (jQuery.browser.opera == true) {
							sooff = 1;
						} else {
							sooff = 2;
						}
					}
				}
				var eleWidth = typeof options.width == "string"|| options.width > 0 ? options.width : $(input).width();
				var eleTop = offset.top + input.offsetHeight + sooff + options.customOffsetTop;
				var isFloat = $("div.mainnav-wrap:first").css("display") == "none" ? false:true;
				if(isFloat){
					eleWidth = (/MSIE [56]/.test(navigator.userAgent))?200:$("div.search-area:first").css("width");
				}
				element.css({
					width : eleWidth,
					top : eleTop,
					left : newleft_ff2 - sooff + newdocoff + options.customOffsetLeft 
				}).css("z-index","100").show();
				if (options.scroll) {
					list.scrollTop(0);
					list.css({
								maxHeight : options.scrollHeight,
								overflow : 'auto'
							});
					if ($.browser.msie
							&& typeof document.body.style.maxHeight === "undefined") {
						var listHeight = 0;
						listItems.each(function() {
									listHeight += this.offsetHeight;
								});
						var scrollbarsVisible = listHeight > options.scrollHeight;
						list.css('height', scrollbarsVisible
										? options.scrollHeight
										: listHeight);
						if (!scrollbarsVisible) {
							listItems.width(list.width()
									- parseInt(listItems.css("padding-left"))
									- parseInt(listItems.css("padding-right")));
						}
					}
				}
			},
			selected : function() {
				var selected = listItems
						&& listItems.filter("." + CLASSES.ACTIVE);
//				return selected && selected.length
//						&& $.data(selected[0], "ac_data");
				return selected && selected.length
						&& {
							data:{doc:selected.text()},
							result:selected.text(),
							value:selected.text()
				};
			},
			emptyList : function() {
				list && list.empty();
			},
			unbind : function() {
				element && element.remove();
			}
		};
	};
	$.Autocompleter.Selection = function(field, start, end) {
	};
})(jQuery);
function auto_complete(input_id, auto_flag) {
	if (auto_flag == 22) {
		isiframe = 1;
		autokind_flag = 2;
	} else if (auto_flag == 23) {
		isso = 1;
		autokind_flag = 2;
	} else {
		autokind_flag = auto_flag;
	}
	$().ready(function() {
		var wid;
		if (isso == 1) {
			wid = 347;
		} else {
			wid = 345
		};
		$(input_id).autocomplete('/SearchSuggest.do?type=search', {
					selectFirst : false,
					max : 10,
					width : wid,
					multiple : false,
					scroll : true,
					scrollHeight : 300,
					highlight : false,
					formatItem : function(data, i, n, value) {
						return "<p>" + value + "</p>";
					}
				});
		var urlstr = window.location.href;
		var iPos = urlstr.indexOf("#");
		if (urlstr.indexOf("#") == -1
				&& urlstr.indexOf("http://www.baike.com/wiki") == -1) {
			$(input_id).click().focus();
		}
	});
}
function auto_complete_doc(input_id, auto_flag) {
	if (auto_flag == 22) {
		isiframe = 1;
		autokind_flag = 2;
	} else if (auto_flag == 23) {
		isso = 1;
		autokind_flag = 2;
	} else if (auto_flag == 24) {
		isnewdoc = 1;
		autokind_flag = 2;
	} else {
		autokind_flag = auto_flag;
	}
	$().ready(function() {
		var wid;
		if (isso == 1) {
			wid = 347;
		} else {
			wid = 345
		};
		$(input_id).autocomplete('/SearchSuggest.do?type=search', {
					selectFirst : false,
					max : 10,
					multiple : false,
					scroll : true,
					scrollHeight : 300,
					highlight : false,
					formatItem : function(data, i, n, value) {
						return "<p>" + value + "</p>";
					}
				});
		var urlstr = window.location.href;
		if (urlstr.indexOf("#") == -1
				&& urlstr.indexOf("http://www.baike.com/wiki") == -1) {
			$(input_id).click().focus();
		}
	});
}
function auto_complete_doc_category(input_id, auto_flag) {
	isnewdoc = 1;
	autokind_flag = auto_flag;
	var id = "#input_" + input_id;
	var wid = 132;
	var ver = jQuery.browser.version;
	if (jQuery.browser.mozilla == true
			|| (jQuery.browser.safari == true
					&& navigator.appVersion.indexOf("Chrome") != -1 && ver >= 530.5)
			|| jQuery.browser.opera == true) {
		wid = 134;
	}
	jQuery(id).autocomplete('/SearchSuggest.do', {
				selectFirst : false,
				max : 10,
				width : wid,
				multiple : false,
				scroll : true,
				scrollHeight : 300,
				highlight : false,
				formatItem : function(data, i, n, value) {
					return "<p>" + value + "</p>";
				}
			});
}
function auto_complete_crossdomain(input_id, auto_flag) {
	if (auto_flag == 22) {
		isiframe = 1;
		autokind_flag = 2;
	} else if (auto_flag == 23) {
		isso = 1;
		autokind_flag = 2;
	} else {
		autokind_flag = auto_flag;
	}
	$().ready(function() {
		var wid;
		if (isso == 1) {
			wid = 347;
		} else {
			wid = 345
		};
		$(input_id)
				.autocomplete(
						'http://www.baike.com/SearchSuggest.do?type=search&callback=?',
						{
							selectFirst : false,
							max : 10,
							width : wid,
							multiple : false,
							scroll : true,
							scrollHeight : 300,
							highlight : false,
							dataType : "jsonp",
							parse : function(data) {
								return $.map(eval(data), function(row) {
											return {
												data : row,
												value : row.doc,
												result : row.doc
											}
										});
							},
							formatItem : function(data, i, n, value) {
								return "<p>" + value + "</p>";
							}
						});
	});
}
function auto_complete_crossdomain_general(input_id, auto_flag, type, width , customOffset,showBing) {
	if (auto_flag == 22) {
		isiframe = 1;
		autokind_flag = 2;
	} else if (auto_flag == 23) {
		isso = 1;
		autokind_flag = 2;
	} else {
		autokind_flag = auto_flag;
	}
	var suffix = "";
	if ("doc" == type) {
		suffix = "type=search&"
	} else if ("category" == type) {
	}
	$().ready(function() {
		var wid;
		if (isso == 1) {
			wid = 347;
		} else {
			wid = width
		}
		$(input_id).autocomplete(
				'http://www.baike.com/SearchSuggest.do?' + suffix
						+ 'callback=?', {
					selectFirst : false,
					max : 10,
					width : wid,
					multiple : false,
					scroll : true,
					scrollHeight : 300,
					highlight : false,
					dataType : "jsonp",
					parse : function(data) {
						return $.map(eval(data), function(row) {
									return {
										data : row,
										value : row.doc,
										result : row.doc
									}
								});
					},
					formatItem : function(data, i, n, value) {
						return "<p>" + value + "</p>";
					},
					customOffsetTop:typeof customOffset === "undefined"?0:customOffset.customOffsetTop,
					customOffsetLeft:typeof customOffset === "undefined"?0:customOffset.customOffsetLeft,
					showBing:typeof showBing === "undefined"?false:showBing
				});
	});
}

var auto_bing = {
	autoElement:{},
	q : "",
	hdList : [],
	request:function (term,data) {
		    var self = this;
		    self.autoElement = $("div.ac_results");
			term = term.toLowerCase();
			self.q = $.trim(term);
			self.hdList = data;
			var req4Bing = document.createElement("script");
			var req = ["http://bj1.api.bing.com/qsonhs.aspx?FORM=HDAUTO&mkt=zh-CN&cp=0&&o=s&type=cb&cb=auto_bing.callback", "&q=",encodeURIComponent(self.q)];
			document.getElementsByTagName("head")[0].appendChild(req4Bing);
			req4Bing.src = req.join("");
	},
	callback:function(data){
		var self = this;
		try {
			var query = "", result = [], d = data.AS;
			query = d.Query;
			if (query != self.q)
				return;
			if(d.FullResults != 1){
				if(self.hdList.length == 0){
					self.autoElement.hide();
				}
				return;
			}
			result = d.Results;
			if(result.length > 0 && result[0].Type=="AS" && result[0].Suggests){
				self.list(result[0].Suggests);
			}
		} catch (e) {
		}
	},
	getLink : function(data){
		var self = this;
		var href = "http://cn.bing.com/search?FORM=HDAUTO&mkt=zh-cn&q="+encodeURIComponent(data.Txt);
		href += "&qs="+data.Type;
		href += "&sk="+data.Sk;
		href += "&pq="+encodeURIComponent(self.q);
		return "<a href=\""+href+"\" target=\"_blank\">"+data.Txt+"</a>";
	},
	list : function(data){
		var self = this;
		var bing_ul = $("ul.bing",self.autoElement);
		bing_ul.html("");
		var length = self.hdList.length;
		if(length == 0){
			bing_ul.addClass("bor-no");
		}else{
			bing_ul.removeClass("bor-no");
		}
		if(length == 0 && data.length==0){
			auto_div.hide();
		}
				
		
		var hdArr = new Array();
		$.each(self.hdList,function(i,n){
			hdArr.push(n.value);
		});
		for(var i = 0; i < data.length; i++){
			if(length >= 10){
				break;
			}
			if($.inArray(data[i].Txt, hdArr) == -1){
				length++;
				var li = $("<li/>").html(self.getLink(data[i])).addClass(length % 2 == 0 ? "ac_even" : "ac_odd").appendTo(bing_ul);
				li.find("a").click(function(){
					self.sendEvent("so_auto_bing_list");
				});
			}
		}
		if(length > self.hdList.length){
			var isFloat = $("div.mainnav-wrap:first").css("display") == "none" ? false:true;
			var litext ="<li class=\"hezuo\""+ (isFloat?" style=\"position:static\">ä»¥ä¸ç±Bingæä¾":">ä»¥ä¸æç¤ºç±Bingå¿åºæç´¢æä¾")+"<span>bing</span></li>";
		     $(bing_ul).prepend(litext);
		     bing_ul.show();
		}else{
			 bing_ul.hide();
		}
	},
	sendEvent:function(eventId){
		if(typeof StatVirtualTraffic =="function"){
			try {
				StatVirtualTraffic(document.referrer, window.location, eventId);
			} 
			catch (e) {
			}
		}
	}
};/**
 * äºå¨å¤´é¨
 * created time:2015-5-19
 * created by author :zwx
 */
(function($) {
	$.HdHead = function(options) {
		return new _Head(options);
	};
	_Head = function(options) {
		this.init(options);
	};
	_Head.prototype = {
		init: function(options) {
			var self = this;
			var defaults = {
				iptForm: "#enter",
				topParent: ".search-panel",
				topIpt:".search-panel :text:first",
				topBtn:".search-button",
				topdocBtn:".entry-button",
				bottomIpt:null,
				bottomBtn:null,
				floatIpt:null,
				floatBtn:null,
				ipt_nav_a:"null",
				productType:"",
				nav:null,
				autocompleteUrl:"http://www.huimg.cn/lib/jquery.autocomplete.search-ext-bing-20160714.js",
				hotwordActionUrl:"http://api.hudong.com/ajaxHotDocCountApi.do?templateId=100000031515&callback=?",
				hotwordUrl:"http://www.huimg.cn/public/js/module/HD_Hots_20160713.js",
				title_filter:null,
				extrafn:null,
				openac:false,
				openhotword:false,
				Hots_config:{}
		    };
		    self.opts = $.extend(defaults, options);
			self.iptForm = $(self.opts.iptForm);
			self.topParent = $(self.opts.topParent);
			self.topIpt = $(self.opts.topIpt);
			self.topBtn = $(self.opts.topBtn);
			self.topdocBtn = $(self.opts.topdocBtn);
			/*åºé¨æç´¢*/
			self.bottomIpt = $(self.opts.bottomIpt);
			self.bottomBtn = $(self.opts.bottomBtn);
			/*æµ®å¨æç´¢*/
			self.floatIpt = $(self.opts.floatIpt);
			self.floatBtn = $(self.opts.floatBtn);
			/* è¾å¥æ¡å¯¼èª*/
			self.ipt_nav_a = $(self.opts.ipt_nav_a);
			/*äº§åç±»å,é»è®¤ä¸ºè¯æ¡*/
			self.productTypes = ['home_doc','body_doc','so_doc','so_tupian'];
			self.productType = self.getProductType(self.productTypes[1],self.opts.productType);
			/*å¯¼èª*/
			self.nav = $(self.opts.nav);
			self.autocompleteUrl = self.opts.autocompleteUrl;
			self.hotwordActionUrl = self.opts.hotwordActionUrl;
			self.hotwordUrl = self.opts.hotwordUrl;
			self.openac = self.opts.openac;
			self.openhotword = self.opts.openhotword;
			self.title_filter = self.opts.title_filter;
			self.extrafn = self.opts.extrafn;
			self.Hots_config = self.opts.Hots_config;
			if(self.iptForm.length>0&&self.topIpt.length>0){
				self.customOffsetLeft = self.iptForm.offset().left-self.topIpt.offset().left+1;
			}
			self.handleEvents();
		},
		getProductType:function(default_ProductType,productType){
			var self = this;
			if ($.inArray(productType, self.productTypes) == -1) {
				return default_ProductType;
			}else{
				return productType;
			}
		},
		handleEvents:function(){
			var self = this;
			 self._topBtnEvent();
			 self._topDocBtnEvent();
			 //self._hoverEvent(self.nav);
			 self._floatEvent();
			 self._bottomEvent();
			 self._ipt_nav_a_Event();
			 self._commonEvent();
			 self._form_submitEvent();
			 self._extraEvent();
		},
		_form_submitEvent:function(){
				/*æç´¢æ¡åè½¦äºä»¶*/
			    var self = this;
				self.iptForm.unbind().submit( function () {
					var p = $.trim($(self.topIpt).val());
					if(self.productType==self.productTypes[0]||self.productType==self.productTypes[1]||self.productType==self.productTypes[2]){
							self.wiki(p);
					}else if(self.productType==self.productTypes[3]){
							self.tupian(p);
					}
					return false;
				} );
		},
		_commonEvent:function(){
			/*éç¨å¤çäºä»¶*/
			var self = this;
			if(self.openac){
				self.loadAutoComplete(1);				
			}
			if(self.openhotword){
				if(""==$.trim(self.hotwordActionUrl)||""==$.trim(self.hotwordUrl)){
					return;
				}
				$.getJSON(self.hotwordActionUrl,function(data){
					self.Hots_config.data = data;
					$.getScript(self.hotwordUrl,function(){
						$.HD_Hots(self.Hots_config);
					});
				});
			}
		},
		_topBtnEvent:function(){
			/*é¡¶é¨è¾å¥æ¡æé®æç´¢å¤çäºä»¶*/
			var self = this;
			if(self.topBtn.length<1||self.topIpt.length<1){
				return;
			}
			self.topBtn.click(function(){
				var p = $.trim($(self.topIpt).val());
				if(self.productType==self.productTypes[0]||self.productType==self.productTypes[1]||self.productType==self.productTypes[2]){
							self.so(p);
					}else if(self.productType==self.productTypes[3]){
							self.tupian(p);
				}
			});
		},
		_topDocBtnEvent:function(){
			/*é¡¶é¨è¾å¥æ¡æé®è¯æ¡å¤çäºä»¶*/
			var self = this;
			if(self.topdocBtn.length<1||self.topIpt.length<1){
				return;
			}
			self.topdocBtn.click(function(){
				var p = $.trim($(self.topIpt).val());
				if(self.productType==self.productTypes[0]||self.productType==self.productTypes[1]||self.productType==self.productTypes[2]){
							self.wiki(p);
					}else if(self.productType==self.productTypes[3]){
							self.tupian(p);
				}
			});
		},
		_hoverEvent:function(nav){
			/*é¡¶é¨å¯¼èªèåå¤çäºä»¶*/
			var self = this;
			if(nav.length<1){
				return;
			}
			self.nav.hover(function(){
				$(this).addClass("hover");
			},function(){
				$(this).removeClass("hover");
			});
		},
		_floatEvent:function(){
			/*æµ®å¨æç´¢è¾å¥æ¡å¤çäºä»¶*/
			var self = this;
			if(self.floatIpt.length<1){
				return;
			}
			self.floatIpt.keydown(function(e){
				if(e.keyCode == 13){
					self.so($(this).val());
					return false;
				}
			});
			if(self.floatBtn.length<1||self.floatIpt.length<1){
				return;
			}
			self.floatBtn.click(function(){
				var q=self.floatIpt.val();
				self.so(q);
			});
		},
		_bottomEvent:function(){
			/*åºé¨æç´¢è¾å¥æ¡å¤çäºä»¶*/
			var self = this;
			if(self.bottomIpt.length<1){
				return;
			}
			self.bottomIpt.keydown(function(e){
				if(e.keyCode == 13){
					self.so($(this).val());
					return false;
				}
			});
			if(self.bottomBtn.length<1||self.bottomIpt.length<1){
				return;
			}
			self.bottomBtn.click(function(){
				var q=self.bottomIpt.val();
				self.so(q);
			});
		},
		_ipt_nav_a_Event:function(){
			/*é¡¶é¨æç´¢è¾å¥æ¡ä¸é¾æ¥ç¹å»äºä»¶*/
			var self = this;
			if(self.ipt_nav_a.length<1){
				return;
			}
			self.ipt_nav_a.click(function(){
				var _this = this;
				var ipt = self.topIpt;
				if(ipt.length>0){
					var ipt_val = $.trim(ipt.val());
					if(ipt_val!=""){
						if(self.isMatherTuPian(_this.href)){
							self.tupian(ipt_val);
						}else{
							self.so(ipt_val);
						}
						return false;
					}
				}
			});
		},
		_extraEvent:function(){
			var self = this;
			if($.isFunction(self.extrafn)){
					self.extrafn();
			}
		},
		isMatherTuPian : function(href){
				var tupianPatrn = 'http:\/\/tupian';
				if (href.match(tupianPatrn)) {
					return true;
				}
				return false;
		},
		loadAutoComplete:function(flag){
			var self = this;
			if(""==$.trim(self.autocompleteUrl)){
				return;
			}
			$.getScript(self.autocompleteUrl,function(){
					/*0 åè½¦ä¸è·³è½¬ï¼1 åè½¦è·³è½¬*/
					isEnterJump = flag;
					isEnterJumpdoc = true;
					self.topIpt.attr("name","q");
					auto_complete_crossdomain_general(self.topIpt, 3, "doc", self.getTopIptFormWidth(), {customOffsetTop:8,customOffsetLeft:self.customOffsetLeft},false);
			});
		},
		so : function(q){
			var self = this;
			var q = $.trim(q);
			if(""==q){
				//alert("è¯·è¾å¥è¯æ¡åï¼");
			}else{
				if($.isFunction(self.title_filter)){
					q = q.replace(/\%/g,"%25");
					q = self.title_filter(q);
					q = q.replace(/\//g,"%2F");
					q = q.replace(/\&/g,"%26");
				}
				location = "http://so.baike.com/doc/"+encodeURIComponent(q)+"&prd=button_doc_search";
			}
		},
		wiki : function(q) {
			var self = this;
			var q = $.trim(q);
                if (q == null || q == "") {
                    //alert("è¯·è¾å¥è¯æ¡åï¼");
                } else {
					if($.isFunction(self.title_filter)){
					q = q.replace(/\%/g,"%25");
					q = self.title_filter(q);
					q = q.replace(/\//g,"%2F");
					q = q.replace(/\&/g,"%26");
				}
                    location = "http://www.baike.com/wiki/" + encodeURIComponent(q) + "&prd=button_doc_entry";
                }
        },
		tupian : function(q){
				var self = this;
				var q = $.trim(q);
				if(""==q){
					//alert("è¯·è¾å¥è¯æ¡åï¼");
				}else{
					if($.isFunction(self.title_filter)){
						q = q.replace(/\%/g,"%25");
					q = self.title_filter(q);
					q = q.replace(/\//g,"%2F");
					q = q.replace(/\&/g,"%26");
					}
					location = "http://so.baike.com/s/tupian/"+encodeURIComponent(q)+"&prd=button_tupian_search";
				}
		},
		getTopIptFormWidth :function (){
			var self = this;
			var inputWidth = self.topIpt.outerWidth();
			if(inputWidth==null||inputWidth==""){
				return 471;
			}
			return  inputWidth;
		}
	};
})(jQuery);