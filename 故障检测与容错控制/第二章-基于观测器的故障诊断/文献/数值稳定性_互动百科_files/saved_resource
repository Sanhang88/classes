//2015-07-01 å¯¹åäº«å¤©æ¶¯ãç½æãç§»å¨å¾®åãé£ä¿¡åä¸çº¿å¤ç
var share={
	commonUrl : 'http://www.baike.com/wiki/' + $("#doctitle_utf8").val(),
    commonmaxlength : 163,
    commoncontent : '',
    urlLength : "",
    commonsummary : '',
	init:function(){
		var self = this;
		self.urlLength = self.commonUrl.length / 2;
		
		self.qqzone();  //QQç©ºé´
		self.tencent(); //è¾è®¯å¾®å
		self.msn();     //MSN
		self.sinalink();   //æ°æµªå¾®å
		self.kaixinlink();  //å¼å¿ç½
	},
		// common share content
	commonContent:function(contentLenth, commonsummaryLength){
		var self = this;
			// common var
		self.commonmaxlength = contentLenth;
		commonablelength = self.commonmaxlength - self.urlLength - $("#doctitle").val().length - 7;
		self.commonsummary = $("#originalsummarycontent").val();
		if (commonablelength > commonsummaryLength) {
			commonablelength = commonsummaryLength;
		}
		if (self.commonsummary.length > commonablelength) {
			self.commonsummary = self.commonsummary.substr(0, commonablelength - 2) + "â¦â¦";
		}
	},
	kaixinlink:function(){
			// ç¹å»è½¬å¸å°Kaixin001
		$('.repaste2kaixin001link').bind("click", function(){
			try {
				recordLog("citiao_fenxiang_kaixin001");
			} 
			catch (e) {
			
			}
			$("body").append("<div id=\"kaixin001_repaste_div\" style=\"display:none;\"><form name=\"kaixin001_repaste\" id=\"kaixin001_repaste\" action=\"http://www.kaixin001.com/repaste/share.php\" method=\"post\" target=\"_blank\"><input type=\"hidden\" name=\"rtitle\" value=\"æå¨äºå¨ç¾ç§ä¸çäºï¼" + $("#doctitle").val() + "\"><input type=\"hidden\" name=\"rcontent\" value=\"" + $("#kaixin001allcontent").val() + "\"><input type=\"hidden\" name=\"rurl\" value=\"" + document.location.href + "\"></form></div>");
			$("#kaixin001_repaste").submit();
			$("#kaixin001_repaste_div").remove();
			return false;
		});
	},
	sinalink:function(){
	
		// æ°æµªå¾®å
		$(".sinaminblog").bind("click", function(){
			try {
				recordLog("citiao_fenxiang_sinaweibo");
			} 
			catch (e) {
			
			}
			var sinaminblogurl = encodeURIComponent('http://www.baike.com/wiki/'+ checkURIDotitle($("#doctitle").val()));
			var maxlength = 140;
			var ablelength = maxlength - sinaminblogurl.length / 2 - $("#doctitle").val().length - 7;
			var summary = $("#originalsummarycontent").val();
			if (ablelength > 50) {
				ablelength = 50;
			}
			if (summary.length > ablelength) {
				summary = summary.substr(0, ablelength - 2) + "â¦â¦";
			}
			var content = $("#doctitle").val() + "  " + summary;
			var html = '<form name="sinaminblogform" id="sinaminblogform" action="http://v.t.sina.com.cn/share/share.php" method="get" target="_blank">' +
			'<input type="hidden" name="title" value="äºå¨ç¾ç§ï¼' +
			content +
			'"><input type="hidden" name="url" value="' +
			sinaminblogurl+
			'"><input type="hidden" name="content" value="utf-8"><input type="hidden" name="ralateUid" value="1649670037"><input type="hidden" name="appkey" value="280937352"></form>';
			$("body").append(html);
			$("#sinaminblogform").submit();
			$("#sinaminblogform").remove();
			return false;
		});
	},
	msn:function(){
	
		// msn
	$(".msn").bind("click", function(){	
		try {
			recordLog("citiao_fenxiang_neteaseweibo");
			} 
		catch (e) {
			}
			var msnblogurl='http://www.baike.com/wiki/' + encodeURIComponent(checkURIDotitle($("#doctitle").val()));
			var maxlength="200";
			var ablelength = maxlength - msnblogurl.length / 2 - $("#doctitle").val().length - 7 ;
			var summary=$("#originalsummarycontent").val();
			var firstimgurl=$("#doc_first_image").val();
			if(ablelength>50){
				ablelength=50;
			}
			if(summary!=null&&summary!=""){
				if(summary.length > ablelength){
					summary = summary.substr(0, ablelength - 2) + "â¦â¦";
				}
			}
			if(firstimgurl==null||firstimgurl==''){
					firstimgurl='';
			}
			var title="äºå¨ç¾ç§ï¼" + $("#doctitle").val(); 
			var url=encodeURI(msnblogurl) + '&title=' + encodeURIComponent(title) + '&description=' + encodeURIComponent(summary) + '&screenshot=' +encodeURIComponent(firstimgurl);
			window.open('http://profile.live.com/badge?url=' + url);	
		});
	},
	tencent:function(){
	
		$(".tencent").bind("click", function(){
			try {
				recordLog("citiao_fenxiang_tencent");
			} 
			catch (e) {
			}
			var self = this;
			share.commonContent(140, 35);
			self.commoncontent = 'äºå¨ç¾ç§ï¼' + $("#doctitle").val() + "  " + self.commonsummary;
			var _t = encodeURIComponent(self.commoncontent);
			var _url = encodeURI('http://www.baike.com/wiki/'+ checkURIDotitle($("#doctitle").val()));
			var _source = encodeURI('1000008');
			var _site = encodeURI('http://www.baike.com');
			var _u = 'http://v.t.qq.com/share/share.php?title=' + _t + '&url=' + _url + '&source=' + _source + '&site=' + _site;
			window.open(_u, 'åäº«å°è¾è®¯å¾®å', 'height=700, width=580, top=0, left=0,toolbar=no, menubar=no, scrollbars=no, resizable=no,location=no, status=no');
		});
	},
	qqzone:function(){
		// qqzone
		$(".qqzone").bind("click", function(){
			try {
				recordLog("citiao_fenxiang_qqzone");
			} 
			catch (e) {
			}
			var self = this;
			share.commonContent(140, 35);
			self.commoncontent = 'äºå¨ç¾ç§ï¼' + $("#doctitle").val() + "  " + self.commonsummary;
			var p = {
				url:location.href,
				summary:self.commoncontent,
				title:"äºå¨ç¾ç§ï¼" + $("#doctitle").val(),
				site:self.commonUrl
			};
			var s = [];
			for(var i in p){
				s.push(i + '=' + encodeURIComponent(p[i]||''));
			}
			var qqzoneurl = "http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?"+s.join('&');
			
			window.open(qqzoneurl, 'åäº«å°qqzone', 'height=700, width=580, top=0, left=0,toolbar=no, menubar=no, scrollbars=no, resizable=yes,location=no, status=no');
			return false;
		});
	}
