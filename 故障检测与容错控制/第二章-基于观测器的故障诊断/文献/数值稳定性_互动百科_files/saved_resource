function newaddthesaurus(){
	if (loaddocThesaurusjsflag) {
		thesaurusIfLock();
	}
	else {
		//jQuery.getScript("http://www.baike.com/wikdoc/sp/st/js/docThesaurus.js", function(){
		jQuery.getScript("http://www.huimg.cn/entry/js/docThesaurus/docThesaurus161012.js", function(){
			thesaurusIfLock();
			loaddocThesaurusjsflag = true;
		}); 
	}
}
