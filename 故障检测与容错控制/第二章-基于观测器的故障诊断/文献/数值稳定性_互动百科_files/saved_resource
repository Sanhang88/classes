var appInfo = {
	appdoctitle: null,
	host: "http://app.baike.com",
	head: document.getElementsByTagName('head').item(0),
	mapeditauth: false,
	initapp: function(doctitle){
		try {
			this.appdoctitle = doctitle;
			this.getMap();
		} 
		catch (e) {
		}
	},
	getMap: function(){
		var url = this.host + "/appmap/getMapInfo?docTitle=" + encodeURI(this.appdoctitle) + '&t=' + Math.random();
		
		HdLoadJs(url, function(){
			appInfo.treatMap();
		});
		/*
		 * var mapscript = document.createElement('script'); mapscript.src =
		 * url; mapscript.type = 'text/javascript'; mapscript.id =
		 * "getmapscript"; mapscript.onreadystatechange = function(){ if
		 * (mapscript.readyState == 'complete' || mapscript.readyState ==
		 * 'loaded') { appInfo.treatMap(); } } mapscript.onload = function(){
		 * appInfo.treatMap(); }; this.head.appendChild(mapscript);
		 */
	},
	treatMap: function(){
		if (typeof(mapJson) != 'undefined' && mapJson != null) {
			var mapeditpower = mapJson.mapEditPow;
			var maplongitude = mapJson.Longitude;
			var maplatitude = mapJson.Latitude;
			
			/**
			 * å¦ææ°æ®å­å¨ï¼åæ¯çæå°å¾æ¨¡åï¼åæ¶çæç¼è¾å°å¾æé®
			 * 
			 * å¦ææ°æ®ä¸å­å¨ï¼åæ¯çææ°å¢å°å¾æé®
			 */
			if (typeof(maplongitude) != 'undefined' && typeof(maplongitude) != 'undefined' && maplongitude != '' && maplatitude != '') {
				// å¦ææ°æ®å­å¨
				var mapdes = mapJson.mapDes;
				var mapzoom = mapJson.mapZoom;
				
				var maphtml = '';
				// maphtml += '<a target="_blank" class="enlargemap"
				// href="http://app.baike.com/map/' +
				// encodeURI(appInfo.appdoctitle) + '">æ¾å¤§å°å¾</a>';
				maphtml += '<div class="mapgoogle a-c" id="mapdiv" style="margin: 10px auto 0pt !important; height:230px; width:280px;"></div>';
				if (mapdes != '') {
					maphtml += "<strong>" + mapdes + "</strong>";
				}
				if (mapeditpower == '1') {
					maphtml = '<a href="javascript:void(0);" class="more" id="editmap" onclick="editMapRequest(\'edit\',\'' + appInfo.appdoctitle + '\',\'' + maplongitude + '\',\'' + maplatitude + '\',\'' + mapdes + '\',\'' + mapzoom + '\'); return false;">ç¼è¾</a>'+maphtml;
				}
				$('#map').append(maphtml);
				
				if($("#MapabcJS").length > 0){
					// å¦ææMapabcçjsï¼åæ§è¡mapabcçæ¾ç¤ºå°å¾åè½
					var jsUrl= "http://www.huimg.cn/entry/js/mapABC.JS.Api.js";
					// ä¸è½½jsï¼ç¼è¾åæ¾ç¤ºå°å¾åè½é½å¨è¿é
					var mapdivId = 'mapdiv';
					if (loadmapABCjsflag) {
						setMapModelForViewDoc(mapdivId, maplongitude, maplatitude, mapzoom);
					}
					else {
						HdLoadJs(jsUrl, function(){
							loadmapABCjsflag = true;
							setTimeout(function(){
								setMapModelForViewDoc(mapdivId, maplongitude, maplatitude, mapzoom);
							}, 1000);
						});
					}
				}else{
					// ä½¿ç¨googleçæ¾ç¤ºè¼æ¯åè½
					var jsUrl= "http://www.huimg.cn/entry/js/mapmanager/mapmanager.js";
					// ä¸è½½jsï¼ç¼è¾åæ¾ç¤ºå°å¾åè½é½å¨è¿é
					if (loadmapmanagerjsflag) {
						setMapInfoForViewDocNew(maplongitude, maplatitude, mapdes, mapzoom, 280, 230);
					}
					else {
						HdLoadJs(jsUrl, function(){
							loadmapmanagerjsflag = true;
							setTimeout(function(){
								setMapInfoForViewDocNew(maplongitude, maplatitude, mapdes, mapzoom, 280, 230);
							}, 1000);
						});
					}
				}
			}
			else {
				// å¦ææ°æ®ä¸å­å¨
				if (mapeditpower == '1') {
					$('#editmore').removeAttr('style');
					$('#editmore').append('<a href="javascript:void(0)" id="addmap">å°å¾</a>');
					$('#addmap').bind("click", function(){
						if (loadmapmanagerjsflag) {
							popMapWindow('add', appInfo.appdoctitle, '116.408198', '39.904667', '', 13);
							return false;
						}
						else {
							jQuery.getScript("/js/mapmanager.js", function(){
								popMapWindow('add', appInfo.appdoctitle, '116.408198', '39.904667', '', 13);
								loadmapmanagerjsflag = true;
								return false;
							});
						}
					});
				}
			}
		}
		$('#getmapscript').remove();
	},
	getStock: function(){
		if ($('#stockinfo').length > 0) {
			var stockCode = $.trim($('#stock_stockcode').val());
			if (stockCode != '') {
				setTimeout(function(){
					try {
						swfobject.embedSWF("http://f0.jrj.com.cn/hq2/hqzs.swf?" + Math.round(Math.random() * 10000, 4), 'stockinfo', '280', '240', "9.0.0", "", {
							first: stockCode + "|S",
							siteid: 'www'
						}, {
							wmode: "opaque",
							AllowScriptAccess: "always"
						});
						if ($('#stockinfo').html() != '') {
							$('#stock').show();
						}
					} 
					catch (e) {
					}
				}, 1000);
			}
		}
	}
};


var navigatorModShowFn={ 
	jsUrl: 'http://www.huimg.cn/entry/js/docNavigatorMod/docNavigatorMod-hangmu.js',	
	init: function(){
		$('.toggleshow').click(function(event){
			$(this).hide();
			$(this).next().show();
			$(this).parent().find("em").removeClass("packup").addClass("tounfold");
			$(this).parent().find('#navData').show();
		});
		$('.togglehide').click(function(){
			$(this).hide();
			$(this).prev().show();
			$(this).parent().find("em").removeClass("tounfold").addClass("packup");
			$(this).parent().find('#navData').hide();
		});
		navigatorModShowFn.tabFn();
	},
	tabFn: function(){
		if (loadNavigatorModFlag) {
			NavigatorModlejs.initNav();		
		} else {
			HdTools.loadJs(navigatorModShowFn.jsUrl, function(){
				loadNavigatorModFlag = true;				
				setTimeout(function(){ NavigatorModlejs.initNav(); }, 1000);
			});
		}
	}	
}

