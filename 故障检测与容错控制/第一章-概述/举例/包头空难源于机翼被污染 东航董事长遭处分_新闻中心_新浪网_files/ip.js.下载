// user
if (typeof(_TT_UP) != 'function') {
    var _TT_UP = function() {
        function _TT_GetCookie(N){
        	var c = document.cookie.split("; ");
        	for (var i = 0; i < c.length; i++) {
        		var d = c[i].split("=");
        		if (d[0] == N) return unescape(d[1]);
        	}
        	return '';
        }

        function _TT_GetPGLS() {
            var r = '';
            try {
                r = document.getElementsByTagName("meta")['stencil'].content;
            } catch (e) {
                r = '';
            }
            return r;
        }
        
        function _TT_GetUP() {
            document.domain = 'sina.com.cn';
            
            var up = _TT_GetCookie('TSGUP');
            if (up) {
                return ';up|' + up;
            }
            var up = _TT_GetCookie('SGUP');
            var r = '';
            if (up && up != '0') {
                var ups = up.split('');
                for (var i = 0; i < ups.length; i++) {
                    if (i < ups.length && i % 6 == 0) {
                        r += ';up|';
                    }
                    if (i % 6 < 4) {
                        r += ups[i];
                    }
                }
            } 
            return r;
        }
        
        function _TT_GetKv() {
            var r = '';
            var pgls = _TT_GetPGLS();
            var up = _TT_GetUP();
            var sg = _TT_GetCookie('SINAGLOBAL');
            
            if (up) {
                r = 'sg|' + sg + ';pgls|' + pgls + up;
            }
            return r;
        }
        
        var _TT_kv = _TT_GetKv();
        if (_TT_kv) {
            var _TT_img = new Image();
            _TT_img.src = 'http://995.adsina.allyes.com/main/adfshow?user=AFP6_for_SINA|vancl_1102|bq_pip_test&db=sina&border=0&local=yes&kv=' + _TT_kv;
            _TT_img.onerror = (_TT_img.onload=(_TT_img.onabort=function(){_TT_img = null;}));
        }
    }
    
    //new _TT_UP();
}


