			try {
				var KM_SINA_CID = "";

				var cookieObj = new Array();
				var cookieItem = new Array();

				cookieObj = document.cookie.split(";");
				for(var j = 0; j < cookieObj.length; j++) {
					cookieItem = cookieObj[j].split("=");
					cookieItem[0] = cookieItem[0].replace(/(^\s+)|(\s+$)/g,"");
					if(cookieItem[0] == "SINAGLOBAL") {
						KM_SINA_CID = cookieItem[1];
					}
				}

